  <!--User Editing allowed BELOW this line - DO NOT DELETE THIS LINE-->
  <xsl:template match="/">
    <tns:CardCrTrnTransitInqRs>
      <xsl:choose>
        <xsl:when test="/web:consultaAutorizacionesTransitoResponse/return/autorizaciones">
          <tns:Status>
            <eoStatus:StatusCode>
              <xsl:value-of select="'M0000'"/>
            </eoStatus:StatusCode>
            <eoStatus:StatusDesc>
              <xsl:value-of select="'OK'"/>
            </eoStatus:StatusDesc>
          </tns:Status>
          <xsl:for-each select="/web:consultaAutorizacionesTransitoResponse/return/autorizaciones">
            <tns:Trn>
              <xsl:if test="autorizacion/text() ">
                <eoTrn:AthId>
                  <xsl:value-of select="autorizacion"/>
                </eoTrn:AthId>
                <eoTrn:CurBalance>
                  <xsl:value-of select='0.0'/>
                </eoTrn:CurBalance>
              </xsl:if>
              <xsl:if test="fecha/text()">
                <eoTrn:OrigDt>
                  <xsl:value-of select='concat (substring (fecha, 1, 4 ), "-", substring (fecha, 5, 2 ), "-", substring (fecha, 7, 2 ) )'/>
                </eoTrn:OrigDt>
              </xsl:if>
              <eoTrn:TotalCurAmt>
                <xsl:if test="monto/text() ">
                  <eoCom:Amt>
                    <xsl:value-of select="monto"/>
                  </eoCom:Amt>
                </xsl:if>
                <xsl:if test="moneda/text() ">
                  <eoCom:CurCode>
                    <xsl:value-of select="moneda"/>
                  </eoCom:CurCode>
                </xsl:if>
              </eoTrn:TotalCurAmt>
              <eoTrn:TrnRef>
                <xsl:value-of select="hora"/>
              </eoTrn:TrnRef>
              <eoTrn:TrnType>
                <eoCom:Type>Autorizaciones</eoCom:Type>
              </eoTrn:TrnType>
            </tns:Trn>
          </xsl:for-each>
        </xsl:when>
        <xsl:otherwise>
          <tns:Status>
            <eoStatus:StatusCode>
              <xsl:value-of select="/web:consultaAutorizacionesTransitoResponse/return/mensajes/@codigo"/>
            </eoStatus:StatusCode>
            <eoStatus:StatusDesc>
              <xsl:value-of select="/web:consultaAutorizacionesTransitoResponse/return/mensajes/@descripcion"/>
            </eoStatus:StatusDesc>
          </tns:Status>
        </xsl:otherwise>
      </xsl:choose>
    </tns:CardCrTrnTransitInqRs>
