                                          <xsl:value-of select="/ns0:PartyAddRq/ns0:Party/ns0:PersonPartyInfo/ns0:Employment/ns0:OrgData/ns0:OrgName/ns0:LegalName"/>
                                       </eoPar:LegalName>
                                    </xsl:if>
                                 </eoPar:OrgName>
                              </xsl:if>
                           </eoPar:OrgData>
                        </xsl:if>
                        <xsl:for-each select="/ns0:PartyAddRq/ns0:Party/ns0:PersonPartyInfo/ns0:Employment/ns0:OtherIncome">
                           <eoPar:OtherIncome>
                              <xsl:if test="ns0:Freq/text()">
                                 <eoPar:Freq>
                                    <xsl:value-of select="ns0:Freq"/>
                                 </eoPar:Freq>
                              </xsl:if>
                              <xsl:if test="ns0:IncomeAmt">
                                 <eoPar:IncomeAmt>
                                    <xsl:if test="ns0:IncomeAmt/appoptcommon:Amt/text()">
                                       <ns2:Amt>
                                          <xsl:value-of select="ns0:IncomeAmt/appoptcommon:Amt"/>
                                       </ns2:Amt>
                                    </xsl:if>
                                    <xsl:if test="ns0:IncomeAmt/appoptcommon:CurCode/text()">
                                       <ns2:CurCode>
                                          <xsl:value-of select="ns0:IncomeAmt/appoptcommon:CurCode"/>
                                       </ns2:CurCode>
                                    </xsl:if>
                                 </eoPar:IncomeAmt>
                              </xsl:if>
                              <xsl:if test="ns0:IncomeLevel/text()">
                                 <eoPar:IncomeLevel>
                                    <xsl:value-of select="ns0:IncomeLevel"/>
                                 </eoPar:IncomeLevel>
                              </xsl:if>
                              <eoPar:IncomeLocation>
                                 <xsl:value-of select="ns0:IncomeLocation"/>
                              </eoPar:IncomeLocation>
                              <xsl:if test="ns0:IncomeSource/text()">
                                 <eoPar:IncomeSource>
                                    <xsl:value-of select="ns0:IncomeSource"/>
                                 </eoPar:IncomeSource>
                              </xsl:if>
                              <xsl:if test="ns0:IncomeType/text()">
                                 <eoPar:IncomeType>
                                    <xsl:value-of select="ns0:IncomeType"/>
                                 </eoPar:IncomeType>
                              </xsl:if>
                           </eoPar:OtherIncome>
                        </xsl:for-each>
                     </eoPar:Employment>
                  </xsl:if>
                  <xsl:if test="/ns0:PartyAddRq/ns0:Party/ns0:PersonPartyInfo/ns0:EmployeeData">
                     <eoPar:EmployeeData>
                        <xsl:if test="/ns0:PartyAddRq/ns0:Party/ns0:PersonPartyInfo/ns0:EmployeeData/ns0:EmployeeNumber/text()">
                           <eoPar:EmployeeNumber>
                              <xsl:value-of select="/ns0:PartyAddRq/ns0:Party/ns0:PersonPartyInfo/ns0:EmployeeData/ns0:EmployeeNumber"/>
                           </eoPar:EmployeeNumber>
                        </xsl:if>
                     </eoPar:EmployeeData>
                  </xsl:if>
               </eoPar:PersonPartyInfo>
            </xsl:if>
            <eoPar:PartyKey>
               <xsl:if test="/ns0:PartyAddRq/ns0:Party/ns0:PartyKey/ns0:PartyType/text()">
                  <eoPar:PartyType>
                     <xsl:value-of select="/ns0:PartyAddRq/ns0:Party/ns0:PartyKey/ns0:PartyType"/>
                  </eoPar:PartyType>
               </xsl:if>
            </eoPar:PartyKey>
         </tns:Party>
      </tns:PartyAddRq>
   </xsl:template>
</xsl:stylesheet>
