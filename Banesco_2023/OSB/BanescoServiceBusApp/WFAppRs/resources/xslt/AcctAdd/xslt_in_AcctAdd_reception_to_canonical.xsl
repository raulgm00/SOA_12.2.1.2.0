                      <xsl:if test="ns0:FSPayee/ns0:PersonData/ns0:Contact/ns0:Locator/text()">
                        <ns3:Locator>
                          <ns3:PostAddr>
                            <xsl:if test="ns0:FSPayee/ns0:PersonData/ns0:Contact/ns0:Locator/ns0:PostAddr/ns0:Country/text()">
