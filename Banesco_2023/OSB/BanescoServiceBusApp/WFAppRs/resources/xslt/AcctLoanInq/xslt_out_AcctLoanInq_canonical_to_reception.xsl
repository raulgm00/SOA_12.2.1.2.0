  <xsl:template match="/">
    <tns:AcctLoanInqRs>
      <xsl:if test="/ns0:AcctLoanInqRs/ns0:Party/text()">
        <tns:Party>
          <tns:PartyKey>
            <xsl:if test="/ns0:AcctLoanInqRs/ns0:Party/ns0:PartyKey/eoPar:PartyId/text()">
              <tns:PartyId>
                <xsl:value-of select="/ns0:AcctLoanInqRs/ns0:Party/ns0:PartyKey/eoPar:PartyId"/>
              </tns:PartyId>
            </xsl:if>
          </tns:PartyKey>
        </tns:Party>
      </xsl:if>
      <tns:Status>
        <appoptcommon:StatusCode>
          <xsl:value-of select="/ns0:AcctLoanInqRs/ns0:Status/eoStatus:StatusCode"/>
        </appoptcommon:StatusCode>
        <appoptcommon:StatusDesc>
          <xsl:value-of select="/ns0:AcctLoanInqRs/ns0:Status/eoStatus:StatusDesc"/>
        </appoptcommon:StatusDesc>
      </tns:Status>
      <xsl:if test="/ns0:AcctLoanInqRs/ns0:AcctLoans">
        <tns:AcctLoans>
          <xsl:for-each select="/ns0:AcctLoanInqRs/ns0:AcctLoans/ns0:AcctLoan">
            <tns:AcctLoan>
              <xsl:if test="eoAcct:AcctSubtype">
                <tns:AcctSubtype>
                  <xsl:if test="eoAcct:AcctSubtype/ns4:Cod/text()">
                    <appoptcommon:Cod>
                      <xsl:value-of select="eoAcct:AcctSubtype/ns4:Cod"/>
                    </appoptcommon:Cod>
                  </xsl:if>
                  <xsl:if test="eoAcct:AcctSubtype/ns4:Desc/text()">
                    <appoptcommon:Desc>
                      <xsl:value-of select="eoAcct:AcctSubtype/ns4:Desc"/>
                    </appoptcommon:Desc>
                  </xsl:if>
                </tns:AcctSubtype>
              </xsl:if>
              <xsl:if test="eoAcct:AcctType">
                <tns:AcctType>
                  <xsl:if test="eoAcct:AcctType/ns4:Cod/text()">
                    <appoptcommon:Cod>
                      <xsl:value-of select="eoAcct:AcctType/ns4:Cod"/>
                    </appoptcommon:Cod>
                  </xsl:if>
                  <xsl:if test="eoAcct:AcctType/ns4:Desc/text()">
                    <appoptcommon:Desc>
                      <xsl:value-of select="eoAcct:AcctType/ns4:Desc"/>
                    </appoptcommon:Desc>
                  </xsl:if>
                </tns:AcctType>
              </xsl:if>
              <xsl:if test="eoAcct:Category/text()">
                <tns:Category>
                  <xsl:value-of select="eoAcct:Category"/>
                </tns:Category>
              </xsl:if>
              <xsl:if test="eoAcct:ClosedDt/text()">
                <tns:ClosedDt>
                  <xsl:value-of select="eoAcct:ClosedDt"/>
                </tns:ClosedDt>
              </xsl:if>
              <!-- Se agrega validacion de club banesco 07/08/2018 -->
              <xsl:if test="eoAcct:ClubBanesco/text()">
                <tns:ClubBanesco>
                  <xsl:value-of select="eoAcct:ClubBanesco"/>
                </tns:ClubBanesco>
              </xsl:if>
              <xsl:if test="eoAcct:CurCode/text()">
                <tns:CurCode>
                  <xsl:value-of select="eoAcct:CurCode"/>
                </tns:CurCode>
              </xsl:if>
              <xsl:if test="eoAcct:Variation">
                <tns:Variation>
                  <xsl:if test="eoAcct:Variation/ns4:Cod/text()">
                    <appoptcommon:Cod>
                      <xsl:value-of select="eoAcct:Variation/ns4:Cod"/>
                    </appoptcommon:Cod>
                  </xsl:if>
                  <xsl:if test="eoAcct:Variation/ns4:Desc/text()">
                    <appoptcommon:Desc>
                      <xsl:value-of select="eoAcct:Variation/ns4:Desc"/>
                    </appoptcommon:Desc>
                  </xsl:if>
                </tns:Variation>
              </xsl:if>
              <xsl:if test="eoAcct:AcctBal">
                <tns:AcctBal>
                  <xsl:if test="eoAcct:AcctBal/eoAcct:CurAmt">
                    <tns:CurAmt>
                      <xsl:if test="eoAcct:AcctBal/eoAcct:CurAmt/ns4:Amt/text()">
                        <appoptcommon:Amt>
                          <xsl:value-of select="eoAcct:AcctBal/eoAcct:CurAmt/ns4:Amt"/>
                        </appoptcommon:Amt>
                      </xsl:if>
                      <xsl:if test="eoAcct:AcctBal/eoAcct:CurAmt/ns4:CurCode/text()">
                        <appoptcommon:CurCode>
                          <xsl:value-of select="eoAcct:AcctBal/eoAcct:CurAmt/ns4:CurCode"/>
                        </appoptcommon:CurCode>
                      </xsl:if>
                    </tns:CurAmt>
                  </xsl:if>
                </tns:AcctBal>
              </xsl:if>
              <xsl:if test="eoAcct:CreditAcctData/eoAcct:CreditAcctPmtInfo/eoAcct:PmtMethod/text()">
                <tns:CreditAcctData>
                  <tns:CreditAcctPmtInfo>
                    <tns:PmtMethod>
                      <xsl:value-of select="eoAcct:CreditAcctData/eoAcct:CreditAcctPmtInfo/eoAcct:PmtMethod"/>
                    </tns:PmtMethod>
                  </tns:CreditAcctPmtInfo>
                </tns:CreditAcctData>
              </xsl:if>
              <xsl:if test="eoAcct:AcctStatus/eoAcct:AcctStatusCode/text()">
                <tns:AcctStatus>
                  <tns:AcctStatusCode>
                    <xsl:value-of select="eoAcct:AcctStatus/eoAcct:AcctStatusCode"/>
                  </tns:AcctStatusCode>
                </tns:AcctStatus>
              </xsl:if>
              <xsl:if test="eoAcct:AcctKey">
                <tns:AcctKey>
                  <xsl:if test="eoAcct:AcctKey/eoAcct:AcctId/text()">
                    <tns:AcctId>
                      <xsl:value-of select="eoAcct:AcctKey/eoAcct:AcctId"/>
                    </tns:AcctId>
                  </xsl:if>
                  <xsl:if test="eoAcct:AcctKey/eoAcct:AcctReference/text()">
                    <tns:AcctReference>
                      <xsl:value-of select="eoAcct:AcctKey/eoAcct:AcctReference"/>
                    </tns:AcctReference>
                  </xsl:if>
                  <xsl:if test="eoAcct:AcctKey/eoAcct:AlternateId/text()">
                    <tns:AlternateId>
                      <xsl:value-of select="eoAcct:AcctKey/eoAcct:AlternateId"/>
                    </tns:AlternateId>
                  </xsl:if>
                </tns:AcctKey>
              </xsl:if>
              <xsl:if test="eoAcct:AcctOpeningInfo">
                <tns:AcctOpeningInfo>
                  <xsl:if test="eoAcct:AcctOpeningInfo/eoAcct:InitialAmt">
                    <tns:InitialAmt>
                      <xsl:if test="eoAcct:AcctOpeningInfo/eoAcct:InitialAmt/ns4:Amt/text()">
                        <appoptcommon:Amt>
                          <xsl:value-of select="eoAcct:AcctOpeningInfo/eoAcct:InitialAmt/ns4:Amt"/>
                        </appoptcommon:Amt>
                      </xsl:if>
                      <xsl:if test="eoAcct:AcctOpeningInfo/eoAcct:InitialAmt/ns4:CurCode/text()">
                        <appoptcommon:CurCode>
                          <xsl:value-of select="eoAcct:AcctOpeningInfo/eoAcct:InitialAmt/ns4:CurCode"/>
                        </appoptcommon:CurCode>
                      </xsl:if>
                    </tns:InitialAmt>
                  </xsl:if>
