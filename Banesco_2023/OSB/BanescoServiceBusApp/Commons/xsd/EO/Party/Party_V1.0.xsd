<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://xmlns.banesco.com/eo/Party_V1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:eoPar="http://xmlns.banesco.com/eo/Party_V1.0" xmlns:eoCom="http://xmlns.banesco.com/eo/Common_V1.0" xmlns:eoBank="http://xmlns.banesco.com/eo/Banking_V1.0" elementFormDefault="qualified"
           xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xs:import namespace="http://xmlns.banesco.com/eo/Banking_V1.0" schemaLocation="../Banking/Banking_V1.0.xsd"/>
	<xs:import namespace="http://xmlns.banesco.com/eo/Common_V1.0" schemaLocation="../Common/Common_V1.0.xsd"/>
	<xs:complexType name="PEPRelated">
		<xs:sequence>
			<xs:element name="Country" type="eoCom:Country" minOccurs="0" maxOccurs="1"/>
			<xs:element name="PersonData" type="eoPar:PersonData" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Información de PEP</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PublicPos" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Public Position</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RelType" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Relation Type</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PEPData">
		<xs:annotation>
			<xs:documentation>Persona Expuesta Políticamente</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="0" maxOccurs="1">
			<xs:element name="Collaborator" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>¿Es usted estrecho colaborador de una persona expuesta políticamente (PEP)?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Family" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>¿Es usted familiar de una persona expuesta políticamente (PEP)?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GovEntity" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Entidad del Gobierno con la que mantiene relación contractual.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GovRelated" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>¿Usted mantiene algún tipo de relación contractual con el Gobierno?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsPEP" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indica si una persona es PEP</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrgIntBelong" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>¿Pertenece usted a un Organismo Internacional?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PeriodStart" type="eoCom:datePattern" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Periodo en que inicia el cargo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PEPRelated" type="eoPar:PEPRelated" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrgMember">
		<xs:annotation>
			<xs:documentation>Organization member</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="0" maxOccurs="1">
			<xs:element name="BirthDt" type="eoCom:datePattern" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Fecha de Nacimiento</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CountryBirth" type="eoCom:Country" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>País de Constitución</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FATCA" type="eoPar:FATCA" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Información de FATCA para staff</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Gender" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Género</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrgEstablishDt" type="eoCom:datePattern" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Fecha de Constitución</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrgName" type="eoPar:OrgName" minOccurs="0" maxOccurs="1"/>
			<xs:element name="PartyData" type="eoPar:PartyData" minOccurs="0" maxOccurs="1"/>
			<xs:element name="PartyKey" type="eoPar:PartyKey" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Customer ID´s</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PEPData" type="eoPar:PEPData" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Información de PEP para staff</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PersonName" type="eoPar:PersonName" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Profession" type="eoCom:ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Profesion.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Shareholding" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Participación accionaria, aplica para accionistas</xs:documentation>
				</xs:annotation>
			</xs:element>
   <xs:element name="SharesType" type="eoCom:CommonType" minOccurs="0"/>
   <xs:element name="StaffPosition" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Cargo, identifica el tipo  de staff

Position Main Staff</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FATCA">
		<xs:annotation>
			<xs:documentation>FATCA</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="0" maxOccurs="1">
			<xs:element name="FatcaType" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Tipo de Institución Fatca.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FormId" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identificación del formulario.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GIIN" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Tipo de identificación GIIN.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GreenCard" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identificación Tipo GreenCard.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OtherNationality" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>¿Tiene usted otra nacionalidad?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SSN" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identificación tipo SSN.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TIN" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identificación tipo TIN.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="US183Permanence" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>¿Ha permanecido usted en los últimos 3 años más de 183 días en los EEUU?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="USRelated" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>¿Está usted vinculado a algún individuo o entidad estadounidense?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="USResid" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>¿Es usted ciudadano o residente de los Estados Unidos de América?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="USSignor" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>¿Es usted firmante autorizado en una entidad americana?</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EmployeeData">
		<xs:annotation>
			<xs:documentation>Specification of employee information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Department" type="eoCom:ValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Department of Employee.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EmployeeNumber" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Número de empleado</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FIData" type="eoBank:FIData" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Information Financial Institution.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SupervisorId" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Supervisor Identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FinancialData">
		<xs:annotation>
			<xs:documentation>The Financial Data aggregate is used to identify monetary figures that can be used to determine a party's financial status</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Desc" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Description</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinancialAmt" type="eoCom:CurAmt" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Financial Amount</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinancialSubType" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Financial SubType
Valid values include: ContributionsToOverhead, Current, Depreciation, Disposable, Dividend, Gross, Investment, Net, Recurring, Tax, Turnover, Other</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinancialType" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Financial Type
Valid values include: Asset, Expense, Income, Other Income, Liability, Revenue, Sales</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Freq" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Frequency
Valid values include: Annually, BiMonthly, Biweekly, Daily, EndOfMonth, FourWeeks, Monthly, Quarterly, SemiAnnually, TwiceMonthly, Weekly</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncomeAmt" type="eoCom:CurAmt" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Amount of the Income.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncomeLevel" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Income Level.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncomeLocation" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Local
Extranjero
Ambos</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncomeSource" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Fuente de Ingresos</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncomeType" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Forma de recibir ingresos

EJ:
Depósito en efectivo. 
Depósito con cheque. 
Depósito mixto (cuasi efectivo).
Transferencias locales.
Transferencias internacionales.

EJ:
Depósito en efectivo. 
Depósito con cheque. 
Depósito mixto (cuasi efectivo).
Transferencias locales.
Transferencias internacionales.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrgName">
		<xs:annotation>
			<xs:documentation>Organization Name.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BusinessName" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Business Name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FullName" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Full company name.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Language" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The value of Language follows the definition of language identifiers in IETF RFC 1766. It is in the form of the two letter language code defined by ISO 639, followed by a hyphen, followed by the two letter country code defined by ISO 3166 or a local dialect name of up to 8 characters. Note that RFC 1766 states that values for Language are case insensitive.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LegalName" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Legal Name.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Name" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Name. Usage depends on context.
Used as a generic name.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PreviousName" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Previous Name</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LoginIdent">
		<xs:annotation>
			<xs:documentation>Login Indetifier</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LoginAuthority" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Login Authority</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LoginName" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The subject's login name</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PartyData">
		<xs:annotation>
			<xs:documentation>Party Data.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Contact" type="eoCom:Contact" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Information Contact.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Nationality" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Nacionalidad</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IssuedIdent" type="eoPar:IssuedIdent" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrgData">
		<xs:annotation>
			<xs:documentation>Organization Data.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eoPar:PartyData">
				<xs:sequence>
					<xs:element name="BusinessType" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Tipo de Negocio</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CountryBirth" type="eoCom:Country" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>País de Origen</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Desc" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Organization Description.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MonthlySalesVol" type="xs:decimal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Nivel Mensual de Ventas</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NumEmployees" type="xs:long" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Number of Employees.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NumShareholder" type="xs:integer" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Número de accionistas</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NumShares" type="xs:double" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Number of Shares</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OrgCategory" type="eoCom:CommonType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Organization Category. Tipo de Persona Juridica</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SalesVolume" type="eoCom:CommonType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Nivel de ventas</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SharesType" type="eoCom:CommonType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Tipo de acciones</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Type" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Type scope of the organization. Eg: Local, National or International.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Staff" type="eoPar:OrgMember" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="OrgName" type="eoPar:OrgName" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Employment">
		<xs:annotation>
			<xs:documentation>Employment data aggregate.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EmploymentCondition" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Employment Condition.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EmploymentStartDt" type="eoCom:datePattern" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Employment Start Date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EmploymentType" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Employment Type.

Valid values include: Casual, Contract, Full, PartTime.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GovermentId" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identificación Contraloría</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="JobTitle" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Job Title.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Occupation" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Occupation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OccupDesc" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Occupation Description.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrevEmploymentEndDt" type="eoCom:datePattern" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Previous Employment End Date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrevEmploymentStartDt" type="eoCom:datePattern" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Previous Employment Start Date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Salary" type="eoCom:CurAmt" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Salary</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SalaryFreq" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Salary Frequency</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SalaryRange" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Salary Range.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TimeFrame" type="eoCom:TimeFrame" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The period of time a person has been employed with this employer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrgData" type="eoPar:OrgData" minOccurs="0" maxOccurs="1"/>
