  <portType name="SoapServicePort">
    <operation name="sayHello">
      <documentation>Returns Hello World as a string.</documentation>
      <input message="tns:sayHelloIn"/>
      <output message="tns:sayHelloOut"/>
    </operation>
    <operation name="getMessage">
      <input message="tns:getMessageIn"/>
      <output message="tns:getMessageOut"/>
    </operation>
    <operation name="getMessagesByDates">
      <input message="tns:getMessagesByDatesIn"/>
