         <xs:element form="qualified" minOccurs="0" name="LineItemVoids" type="POSLogTotals">
            <xs:annotation>
               <xs:documentation>The monetary value of  the sum of all line item voids recorded
                        at the Till</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="PostTransactionVoids" type="POSLogTotals">
