         <xs:documentation>(AAIA - PIES - Indicates background color/type for
				Digital
				Assets.)
			</xs:documentation>
      </xs:annotation>
      <xs:union memberTypes="BackgroundColorTypeEnumeration xs:string TypeCodeExtension"/>
   </xs:simpleType>
   <xs:simpleType name="BackgroundColorTypeEnumeration">
      <xs:restriction base="xs:NMTOKEN">
         <xs:enumeration value="White"/>
         <xs:enumeration value="WhiteWithClippingPath"/>
         <xs:enumeration value="Transparent"/>
         <xs:enumeration value="Other"/>
         <xs:enumeration value="NULL"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="CameraPerspectiveType">
      <xs:annotation>
         <xs:documentation>(AAIA - PIES - Product Orientation as shown in
				Image Digital
				Assets.)
