<?xml version='1.0' encoding='UTF-8'?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://mx.agarcia.ea/Commons/ProductContentManagement/v1" xmlns="http://mx.agarcia.ea/Commons/ProductContentManagement/v1" xmlns:enums="http://mx.agarcia.ea/Commons/Enums/v1" elementFormDefault="qualified" attributeFormDefault="unqualified">
   <xs:import schemaLocation="CommonEnums.xsd" namespace="http://mx.agarcia.ea/Commons/Enums/v1"/>
   <xs:complexType name="PCMBase">
      <xs:annotation>
         <xs:documentation>Asset plus data about the asset, i.e. location,
				provider,
				etc
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" minOccurs="0" name="PCMAssetDescription" type="xs:string">
            <xs:annotation>
               <xs:documentation>Text description for the content
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="PCMAssetName" type="xs:string">
            <xs:annotation>
               <xs:documentation>Name of this asset</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element form="qualified" name="ItemID" type="ItemIDCommonData">
               <xs:annotation>
                  <xs:documentation>Unique item identifier for this asset
						</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element form="qualified" name="PCMUUID" type="UUIDType">
               <xs:annotation>
                  <xs:documentation>Unique identifier for this specific
							asset
						</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element form="qualified" name="MerchandiseHierarchy" type="MerchandiseHierarchyCommonData">
               <xs:annotation>
                  <xs:documentation>The location in the merchandise
							structure
						</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
         <xs:element form="qualified" minOccurs="0" name="AssetGroupID" type="xs:string">
            <xs:annotation>
               <xs:documentation>Structure for defining a collection of assets.
						Could provide
						for nesting of multiple assets to be associated with
						item(s) - can
						occur
						multiple times
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="BusinessAssociation" type="BusinessAssociationType">
            <xs:annotation>
               <xs:documentation>Identifies business associations between PCM
						Assets, for
						example, an item is associated with a warranty
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="DigitalAsset" type="DigitalAssetType">
            <xs:annotation>
               <xs:documentation>THE DIGITAL ASSET</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="DigitalAssetList" type="DigitalAssetListType">
            <xs:annotation>
               <xs:documentation>List of digital assets, used with a request or
						response for a
						list of digital assets.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Collateral" type="CollateralType">
            <xs:annotation>
               <xs:documentation>Supporting information, usually sales or
						marketing
						information.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="ContactList" type="ContactListType">
            <xs:annotation>
               <xs:documentation>List of parties to be contacted
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="FileContent" type="FileContentType">
            <xs:annotation>
               <xs:documentation>File(s) and location(s) to retrieve this PCM
						Asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="MissingData" type="xs:string">
            <xs:annotation>
               <xs:documentation>Identifies missing data from this request
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ProviderList" type="ProviderListType">
            <xs:annotation>
               <xs:documentation>List of parties providing access to this PCM
						Asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ReasonCode" type="ReasonCodeCommonData"/>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="SellingAssociation" type="SellingAssociationType">
            <xs:annotation>
               <xs:documentation>Identifies relationship between this PCM Asset
						and others, for
						example. upselling opportunities
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
      </xs:sequence>
      <xs:attribute form="unqualified" name="AcknowledgeFlag" type="xs:boolean">
         <xs:annotation>
            <xs:documentation>Flag to acknowledge receipt and understanding what
					to do with the
					asset(s) (update/deactivate)
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="UpdateType" type="UpdateTypeCode">
         <xs:annotation>
            <xs:documentation>Identifies what portion is being updated, for
					example,
					Metadata
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="AllDataFlag"/>
   </xs:complexType>
   <xs:complexType name="AssetSceneContextType">
      <xs:annotation>
         <xs:documentation>Information about the structure of the asset, e.g.
				background
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" name="Collection" type="xs:string">
            <xs:annotation>
               <xs:documentation>For example, family, line, cross-sell
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" name="Setting" type="xs:string">
            <xs:annotation>
               <xs:documentation>For example, Propped, limbo, etc.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" name="Environment" type="xs:string">
            <xs:annotation>
               <xs:documentation>for example, Indoor, outdoor, studio,
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute form="unqualified" name="Type" type="AssetSceneTypeCode">
         <xs:annotation>
            <xs:documentation>Collection, Solo</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="InUseFlag" type="xs:boolean"/>
   </xs:complexType>
   <xs:complexType name="AssetVersionManagementType">
      <xs:annotation>
         <xs:documentation>Information about this version of the asset
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" minOccurs="0" name="PCMUUID" type="UUIDType">
            <xs:annotation>
               <xs:documentation>Unique identifier for digital asset for external
						reference
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ReasonDiscontinued" type="xs:string">
            <xs:annotation>
               <xs:documentation>A notification to the user of an asset that an
						asset is no
						longer valid and the reason why. May include a
						replacement indicator
						that a
						more current asset is available or may
						be a reminder that the asset
						Shelf
						Life is at its end.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ReplacementFor" type="ReplacementForType">
            <xs:annotation>
               <xs:documentation>New Content intended to replace existing
						asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ReplaceOnDateTime" type="xs:dateTime">
            <xs:annotation>
               <xs:documentation>Some future date upon which a currently in-use
						asset should be
						replaced with a newer asset that is pending and
						should not be released
						until
						the replace date.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="BusinessAssociationType">
      <xs:annotation>
         <xs:documentation>Identifies business associations between PCM
				Assets, for example, an
				item is associated with a warranty
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:choice maxOccurs="unbounded">
            <xs:element form="qualified" name="ItemID" type="ItemIDCommonData">
               <xs:annotation>
                  <xs:documentation>The PCM Asset relates to this particular item
						</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element form="qualified" name="PCMUUID" type="UUIDType">
               <xs:annotation>
                  <xs:documentation>This PCM Asset relates to This other PCM
							Asset
						</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element form="qualified" name="MerchandiseHierarchy" type="MerchandiseHierarchyCommonData">
               <xs:annotation>
                  <xs:documentation>This PCM Asset relates to this level in the
							merchandise
							hierarchy
						</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Usage" type="xs:string">
            <xs:annotation>
               <xs:documentation>identifies how this PCM Asset relates to the
						Item, PCM Asset
						or Merchandise Hierarchy defined herein
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="CollateralType">
      <xs:annotation>
         <xs:documentation>Marketing data about this asset</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Feature" type="xs:string"/>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Benefit" type="xs:string"/>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="MarketingInformation" type="xs:string">
            <xs:annotation>
               <xs:documentation>Things like, International, United States,
						Canada,
						Global
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Story" type="xs:string"/>
         <xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ColorModelType">
      <xs:sequence>
         <xs:element form="qualified" minOccurs="0" name="BackgroundColor" type="BackgroundColorType">
            <xs:annotation>
               <xs:documentation>Color of the image background</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ColorCalibration" type="xs:string">
            <xs:annotation>
               <xs:documentation>Standard of color calibration used (Kodak,
						Heidelberg CMM,
						Apple ColorSync, etc)
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ColorFamily" type="xs:string"/>
         <xs:element form="qualified" minOccurs="0" name="Contrast" type="xs:string">
            <xs:annotation>
               <xs:documentation>The difference in brightness between light and
						dark areas of
						an image. Contrast determines the number of shades in
						the
						image.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Description" type="xs:string">
            <xs:annotation>
               <xs:documentation>Text description of the color model used in this
						asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute form="unqualified" name="ColorFlag" type="xs:boolean">
         <xs:annotation>
            <xs:documentation>true - Color vs. false - Black - White
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="ColorScheme" type="DAMColorScheme">
         <xs:annotation>
            <xs:documentation>Type of color scheme used in encoding asset - RGB,
					RGBA, YcbCr,
					CMY, CMYK, HSV, HMMD, Monochrome
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="ConstrainProportionsType">
      <xs:sequence>
         <xs:element form="qualified" name="PrintWidth" type="AspectType">
            <xs:annotation>
               <xs:documentation>Pint width of the asset</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" name="PrintHeight" type="AspectType">
            <xs:annotation>
               <xs:documentation>Print height of the asset</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ContactListType">
      <xs:annotation>
         <xs:documentation>List of parties to contact</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" maxOccurs="unbounded" name="Contact" type="ContactCommonData"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DigitalAssetType">
      <xs:annotation>
         <xs:documentation>THE DIGITAL ASSET</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element form="qualified" name="PCMUUID" type="UUIDType">
               <xs:annotation>
                  <xs:documentation>Unique identifier for this digital
							asset.
						</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element form="qualified" name="ItemID"/>
         </xs:choice>
         <xs:element form="qualified" minOccurs="0" name="DigitalAssetContent" type="DigitalAssetContentType">
            <xs:annotation>
               <xs:documentation>Information about the items in The Digital
						Asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="DigitalAssetData" type="DigitalAssetDataType">
            <xs:annotation>
               <xs:documentation>Information about the structure of The Digital
						Asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="DigitalAssetDate" type="DigitalAssetDateType">
            <xs:annotation>
               <xs:documentation>Date information about this asset, i.e. creation,
						expiration,
						etc
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="DigitalAssetFileName" type="xs:string">
            <xs:annotation>
               <xs:documentation>Filename which contains the asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="DigitalAssetFileFormat" type="xs:string">
            <xs:annotation>
               <xs:documentation>file format </xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="DigitalAssetVersion" type="xs:string">
            <xs:annotation>
               <xs:documentation>Version of this Digital Asset</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Description" type="DescriptionCommonData">
            <xs:annotation>
               <xs:documentation>Description of this Asset</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Entitlement" type="EntitlementType">
            <xs:annotation>
               <xs:documentation>Information about rights to The Digital
						Asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Media" type="PCMMediaType">
            <xs:annotation>
               <xs:documentation>The location where the other industry standard's
						meta-data is
						placed.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="MerchandiseHierarchy" type="MerchandiseHierarchyCommonData">
            <xs:annotation>
               <xs:documentation>A level in the Merchandise Structure
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="OriginOfTheAsset" type="OriginOfTheAssetType">
            <xs:annotation>
               <xs:documentation>Where the asset originated</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Representation" type="RepresentationTypeCode">
            <xs:annotation>
               <xs:documentation>AAIA - PIES - identifes if the content of this
						digital asset
						contains a representation of the class of the real
						item.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="SellingAssociation" type="SellingAssociationType">
            <xs:annotation>
               <xs:documentation>Identifies relationship between this PCM Asset
						and others, for
						example. upselling opportunities
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
      </xs:sequence>
      <xs:attribute default="Approved" form="unqualified" name="AssetStatus" type="AssetStatusTypeCode" use="optional">
         <xs:annotation>
            <xs:documentation>The current status of this asset
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="PrimaryFlag" type="xs:boolean">
         <xs:annotation>
            <xs:documentation>In the case of multiple assets for a particular
					item(s), this is
					the primary asset for this item(s).
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="DigitalAssetContentType">
      <xs:annotation>
         <xs:documentation>Information about the items in the asset
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="AccessoriesInAsset" type="xs:string">
            <xs:annotation>
               <xs:documentation>List if visible accessories in the asset. Do
						tags, cases,
						cords, battery packs, etc ... show?
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Annotation" type="xs:string">
            <xs:annotation>
               <xs:documentation>Provide structured, free text, or keywords
						(Replaces
						ProductDescription, MerchandiseView, Personalization,
						TargetMedia)
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="AssetSceneContext" type="AssetSceneContextType">
            <xs:annotation>
               <xs:documentation>Description of the context for the scene.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="CameraPerspective" type="CameraPerspectiveType">
            <xs:annotation>
               <xs:documentation>For the entire digital asset (bullseye, front,
						side…3/4…)
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Campaign" type="xs:string">
            <xs:annotation>
               <xs:documentation>A program of advertising and marketing activities
						to arouse
						interest in a coming event. This could be an MPEG
						reference to a picture
						shot for a specific marketing or advertising
						campaign.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="IntendedUsage" type="xs:string">
            <xs:annotation>
               <xs:documentation>golf, beach – how is this asset intended to be
						used
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="ItemInAsset" type="ItemInAssetType">
            <xs:annotation>
               <xs:documentation>A list of the items identified in this
						asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ItemsPerAsset" type="xs:integer">
            <xs:annotation>
               <xs:documentation>The number of items in the asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Lifestyle" type="xs:string">
            <xs:annotation>
               <xs:documentation>Identifies the lifestyles to which this asset can
						be use. For
						example type, value…psychographic, demographic, age.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="PersonalizationMatchCode" type="xs:string">
            <xs:annotation>
               <xs:documentation> it provides a basis for personalization engines
						such as those
						used in websites to know which of ‘n’ available
						digital assets of an
						item to
						use based on that personalization code
						eg; if the website knows that
						I’m a
						surfer dude, then show the
						swimsuit on a gen-x babe with natty blonde
						hair
						and a puka shell
						necklace rather than in cabana at the Beverly Hills
						Hotel
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element form="qualified" name="PricePoint">
               <xs:annotation>
                  <xs:documentation>The predefined list of price points between two
							actors –
							such as value, premium, etc
						</xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:simpleContent>
                     <xs:extension base="AmountCommonData">
                        <xs:attribute default="true" form="unqualified" name="MaximumFlag" type="xs:boolean">
                           <xs:annotation>
                              <xs:documentation>Minimum/Maximum Flag</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:extension>
                  </xs:simpleContent>
               </xs:complexType>
            </xs:element>
            <xs:element form="qualified" name="PriceRange">
               <xs:annotation>
                  <xs:documentation>Minimum: Maximum range of price for an item
							indicated in
							an asset
						</xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:simpleContent>
                     <xs:extension base="AmountCommonData">
                        <xs:attribute default="true" form="unqualified" name="MaximumFlag" type="xs:boolean">
                           <xs:annotation>
                              <xs:documentation>Minimum/Maximum Flag</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:extension>
                  </xs:simpleContent>
               </xs:complexType>
            </xs:element>
         </xs:choice>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Relationship" type="xs:string">
            <xs:annotation>
               <xs:documentation>Relationship model to associate item data to
						asset(s)
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Season" type="xs:string">
            <xs:annotation>
               <xs:documentation>such as Holiday</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ShotsPerAsset" type="xs:integer">
            <xs:annotation>
               <xs:documentation>Single v Multiple count</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="SuggestedRetailPrice" type="PriceCommonData">
            <xs:annotation>
               <xs:documentation>the price of the merchandise at retail
						recommended by the
						manufacturer to the retailer
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Theme" type="xs:string">
            <xs:annotation>
               <xs:documentation>the price of the merchandise at retail
						recommended by the
						manufacturer to the retailer
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ViewDescription" type="xs:string">
            <xs:annotation>
               <xs:documentation>Standard cinematic description used in
						storyboarding or art
						direction as a general indication of the view
						– examples Close Up, Modem,
						Right Angle, Left Angle, High Angle,
						Down View, Wide Shot.
						Combinations of
						these are possible, such as
						Right Angle Medium Shot.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute form="unqualified" name="AssembledFlag" type="xs:boolean">
         <xs:annotation>
            <xs:documentation>Yes – Assembled No – unassembled merchandise
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="HumanModelsFlag" type="xs:boolean">
         <xs:annotation>
            <xs:documentation>Yes – Humans are in the Asset No – No Humans are
					in the
					Asset
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="ShotSize" type="ShotSizeTypeCode">
         <xs:annotation>
            <xs:documentation>Identifies how large the characters and objects
					featured in the
					asset will appear.
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="DigitalAssetDataType">
      <xs:annotation>
         <xs:documentation>Information about the asset</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" minOccurs="0" name="AdditionalInformation" type="xs:string">
            <xs:annotation>
               <xs:documentation>AAIA - PIES - User Free-Form information about
						Digital
						Asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="CreationProgram" type="xs:string">
            <xs:annotation>
               <xs:documentation>The software program used to create this
						asset.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="RecommendUse" type="xs:string">
            <xs:annotation>
               <xs:documentation>In order to simplify the characteristics of the
						digital assets
						(it is the asset more used) the message contains a
						“recommended use”
						element
						that is easy to understand by both parts
						and does not require to be an
						expert in the complexity of the data.
						Example, the Metadata says
						the asset
						has a resolution 1775 x 1230
						pixels, color space RGB 8 bit by color,
						format
						TIF or JPG compressed
						to maximum quality… that set implies that the
						digital
						asset only can
						be used to print in maximum quality paper and can be
						published in
						the web. Then the” recommended use” data will be
						“Print maximum
						quality” and “web publishing”. On the contrary, if the digital
						asset has a
						100 x 100 pix, JPG low quality, the “recommended use”
						will be only
						“web
						publishing”. This element’s intention is to
						“translate” complicated
						characteristics into a simpler attribute
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="TargetMedia" type="xs:string">
            <xs:annotation>
               <xs:documentation>Such as, POP, Web, Catalog, DM, Circular … (Not
						an
						enumeration)
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="TextNotes" type="xs:string">
            <xs:annotation>
               <xs:documentation>Generic notes about the asset</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Title" type="DescriptionCommonData">
            <xs:annotation>
               <xs:documentation>Creator’s name for this asset</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:choice>
            <xs:element form="qualified" name="DocumentData" type="DocumentDataType"/>
            <xs:element form="qualified" name="DynamicData" type="DynamicDataType"/>
            <xs:element form="qualified" name="ImageData" type="ImageDataType"/>
            <xs:element form="qualified" name="UnstructuredData" type="UnstructuredDataType"/>
         </xs:choice>
         <xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
      </xs:sequence>
      <xs:attribute default="false" form="unqualified" name="AccessoryFlag" type="xs:boolean" use="optional">
         <xs:annotation>
            <xs:documentation>Indicates this data is about an accessory to the
					asset, i.e. logo,
					graphic, embroidery, finishing touches, etc.
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute default="true" form="unqualified" name="DefaultFlag" type="xs:boolean" use="optional"/>
      <xs:attribute default="OriginalPhoto" form="unqualified" name="DigitalDataType" type="DigitalDataTypeCode" use="optional">
         <xs:annotation>
            <xs:documentation>OriginalPhoto, ProductLarge, ProductThumbnail,
					ProductMixMatch,
					ProductFlashPix, OutfitLarge, OutfitSmall,
					OutfitThumbnail,
					ColorSwatchThumbnail, HomePageLarge, HomePageSmall,
					CompanyLogo
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute default="ProductOnly" form="unqualified" name="SceneContext" type="PCMSceneContext" use="optional">
         <xs:annotation>
            <xs:documentation>ProductOnly, ProductWithAccessories,
					ProductColorSwatches,
					MultipleProducts, ProductWithModel (note issue
					with model
					releases)
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute default="Catalog" form="unqualified" name="Status" type="AssetDataStatusTypeCode" use="optional">
         <xs:annotation>
            <xs:documentation> Prototype, LineConfirm, Catalog
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="DigitalAssetDateType">
      <xs:annotation>
         <xs:documentation>Dates for Approve, Create, Effective, Expire,
				Release, Discontinue,
				ShotTaken
			</xs:documentation>
      </xs:annotation>
      <xs:simpleContent>
         <xs:extension base="xs:date">
            <xs:attribute form="unqualified" name="TypeCode" type="AssetDateTypeCode">
               <xs:annotation>
                  <xs:documentation>The type of this date</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
   <xs:complexType name="DigitalAssetListType">
      <xs:annotation>
         <xs:documentation>List of assets returned in response to a request
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="DigitalAsset" type="DigitalAssetType">
            <xs:annotation>
               <xs:documentation>The Digital Asset</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute form="unqualified" name="ResponseCode" type="AssetResponseCode">
         <xs:annotation>
            <xs:documentation>Yes, No, YesButCostExtra, WillCreate,
					WillNotCreate
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="DocumentDataType">
      <xs:annotation>
         <xs:documentation>Details about the document asset</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" minOccurs="0" name="AssetSize">
            <xs:complexType>
               <xs:simpleContent>
                  <xs:extension base="DecimalCommonData">
                     <xs:attribute form="unqualified" name="UnitOfMeasure" type="UnitOfMeasureCodeCommonData"/>
                  </xs:extension>
               </xs:simpleContent>
            </xs:complexType>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Display" type="DisplayCommonData"/>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Language" type="LanguageCode">
            <xs:annotation>
               <xs:documentation>A particular document can contain multiple
						languages, i.e. an
						instruction sheet may be printed in English and
						Spanish
						simultaneously
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
      </xs:sequence>
      <xs:attribute default="PDF" form="unqualified" name="DocumentAssetFormat" type="DocumentAssetTypeCode">
         <xs:annotation>
            <xs:documentation>Such as, Word, PDF</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="DocumentType" type="DocumentationTypeCode">
         <xs:annotation>
            <xs:documentation>Such as Warranty, Features,
					MaterialSafetyDataSheet
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="DynamicDataType">
      <xs:annotation>
         <xs:documentation>Details about the Audio, Video or Animation asset
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" minOccurs="0" name="AspectRatio" type="xs:string">
            <xs:annotation>
               <xs:documentation>The width-to-height ratio used to determine how
						an image fits
						on a page or monitor.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="AssetSize">
            <xs:complexType>
               <xs:simpleContent>
                  <xs:extension base="DecimalCommonData">
                     <xs:attribute form="unqualified" name="UnitOfMeasure" type="UnitOfMeasureCodeCommonData"/>
                  </xs:extension>
               </xs:simpleContent>
            </xs:complexType>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Compression" type="xs:string">
            <xs:annotation>
               <xs:documentation>An indication of the compression method used to
						reduce the
						file size. Examples include quick time, real player,
						mpg,
						etc.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="CreativeDirection" type="xs:string"/>
         <xs:element form="qualified" minOccurs="0" name="Format" type="xs:string">
            <xs:annotation>
               <xs:documentation>An indication of animation format used to create
						the motion,
						could be gif, pics, flash, etc
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="OptimalMediaPlayer" type="xs:string">
            <xs:annotation>
               <xs:documentation>The media player to which this asset was targeted
						(if any)
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="PlaybackRate" type="xs:string">
            <xs:annotation>
               <xs:documentation>The targeted frame rate for the compression rate
						of display of
						the animation, usually expressed in number of frames
						per
						second
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Resolution" type="ResolutionType"/>
         <xs:element form="qualified" minOccurs="0" name="Runtime">
            <xs:annotation>
               <xs:documentation>The running length of the dynamic asset at the
						playback
						rate
					</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:simpleContent>
                  <xs:extension base="DecimalCommonData">
                     <xs:attribute default="Seconds" form="unqualified" name="UnitOfMeasureCode" type="UnitOfMeasureCodeCommonData"/>
                  </xs:extension>
               </xs:simpleContent>
            </xs:complexType>
         </xs:element>
         <xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
      </xs:sequence>
      <xs:attribute form="unqualified" name="DynamicAssetFormat" type="DynamicAssetFormatType"/>
      <xs:attribute form="unqualified" name="Language" type="LanguageCode"/>
      <xs:attribute form="unqualified" name="StreamingFlag" type="xs:boolean" use="optional">
         <xs:annotation>
            <xs:documentation>An indicator that the file is compressed to start
					display before
					fully uncompressing and an indication of which
					compression method used
					to
					achieve streaming capability.
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="TypeCode" type="DynamicAssetTypeCode">
         <xs:annotation>
            <xs:documentation>Animation, Audio, Video</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="EntitledLocationType">
      <xs:annotation>
         <xs:documentation>One or more locations at which the entitlement, in
				the form of a
				restriction or permission, apply.
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" maxOccurs="unbounded" name="Location" type="xs:string"/>
      </xs:sequence>
      <xs:attribute form="unqualified" name="PermissionFlag" type="xs:boolean">
         <xs:annotation>
            <xs:documentation>restriction/permission</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="EntitledPartyType">
      <xs:annotation>
         <xs:documentation>Information about the parties access to this asset
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" name="Partner" type="xs:string"/>
         <xs:element form="qualified" minOccurs="0" name="ChannelCharge" type="AmountCommonData">
            <xs:annotation>
               <xs:documentation>The charge this channel must pay to use this
						asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute form="unqualified" name="Access" type="EntitledPartiesAccessType">
         <xs:annotation>
            <xs:documentation>HasAccess, CanSeeOnly, NoAccess</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="EntitlementType">
      <xs:annotation>
         <xs:documentation>Information about the rights to the asset
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="PCMUUID" type="UUIDType"/>
         <xs:element form="qualified" minOccurs="0" name="AssetVersionManagement" type="AssetVersionManagementType"/>
         <xs:element form="qualified" minOccurs="0" name="StartDate" type="xs:dateTime">
            <xs:annotation>
               <xs:documentation>The month, day and year when an asset can first
						be used in any
						media.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="EndDate" type="xs:dateTime">
            <xs:annotation>
               <xs:documentation>The month, day and year when an asset can no
						longer be used in
						any media.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Copyright" type="xs:string">
            <xs:annotation>
               <xs:documentation>Copyright notice</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Disclaimer" type="xs:string"/>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Party" type="PartyType"/>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Price" type="PriceType"/>
         <xs:element form="qualified" minOccurs="0" name="Rights" type="xs:string">
            <xs:annotation>
               <xs:documentation>Structure for Digital Rights Management (DRM)
						This could be ID
						and location of external IPMP system for lookup of
						usage rights
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Usage" type="xs:string">
            <xs:annotation>
               <xs:documentation> to report usage of content to owner /
						source
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="UsageRestriction" type="UsageRestrictionType"/>
         <xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
      </xs:sequence>
      <xs:attribute form="unqualified" name="Status" type="EntitlementStatusTypeCode">
         <xs:annotation>
            <xs:documentation>Active, Inactive, Discontinued, Changed, Replaced,
					AssetRightsExpired
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="PurgeAssetFlag" type="xs:boolean">
         <xs:annotation>
            <xs:documentation>Archive vs. Production Database To purge or not to
					purge digital
					assets? (State Yes or No)
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="EditFlag">
         <xs:annotation>
            <xs:documentation>Whether or not the user of the asset has
					permission to change or
					modify the asset from its original form
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="FileContentType">
      <xs:annotation>
         <xs:documentation>Information about access to the asset, i.e. file
				locations,
				etc
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" minOccurs="0" name="AssetCount" type="xs:integer">
            <xs:annotation>
               <xs:documentation>A count of the number of assets in this
						file
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="AssetFileSize" type="xs:integer"/>
         <xs:element form="qualified" minOccurs="0" name="AssetFileFormat" type="xs:string">
            <xs:annotation>
               <xs:documentation> Encoding method used to store asset. The W3C
						site suggests,
						http://www.w3.org/Daemon/User/Config/Suffixes.html,
						the list of content
						types can be extended.\
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="AssetInFile">
            <xs:annotation>
               <xs:documentation>List of the assets in this file
					</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:choice maxOccurs="unbounded">
                     <xs:element form="qualified" name="PCMUUID" type="UUIDType"/>
                     <xs:element form="qualified" name="ItemID" type="ItemIDCommonData"/>
                  </xs:choice>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="DelayDate" type="xs:date">
            <xs:annotation>
               <xs:documentation>The date this file will be available
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="DeliveryDate" type="xs:date">
            <xs:annotation>
               <xs:documentation>The date this file will be delivered
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="DigitalSignature" type="xs:base64Binary">
            <xs:annotation>
               <xs:documentation>See RFC 3275 (optional) (future)
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="FileDateModified" type="xs:date">
            <xs:annotation>
               <xs:documentation>AAIA - PIES - Last date of modification to
						defined Digital
						Asset file.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="URI" type="URIType">
            <xs:annotation>
               <xs:documentation>Uniform Resource Identifier to identify where the
						file is
						located
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute form="unqualified" name="Comparator" type="ComparatorTypeCode">
         <xs:annotation>
            <xs:documentation>Used in Requests for information before, after,..
					a particular
					criteria
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="Compression" type="FileCompressionTypeCode">
         <xs:annotation>
            <xs:documentation>The method used to compress the digital asset for
					example, JPEG,
					JPEG2000, LZW, LZ77, RLE, Uncompressed
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="TransmissionMedium" type="TransmissionMediumTypeCode">
         <xs:annotation>
            <xs:documentation>How information is available for transmission.
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="ImageAssetSizeType">
      <xs:annotation>
         <xs:documentation>The number of pixels along the width and height of
				an image. This is a
				measure of the amount of image data in the photo,
				not its physical size
				when printed
				or displayed on a monitor.
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" minOccurs="0" name="ConstrainProportions" type="ConstrainProportionsType"/>
         <xs:element form="qualified" minOccurs="0" name="PixelWidth" type="AspectType">
            <xs:annotation>
               <xs:documentation>Number of Pixels along the width of the
						image
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="PixelHeight" type="AspectType">
            <xs:annotation>
               <xs:documentation>Number of pixels along the height of the
						image
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ResampleAsset">
            <xs:annotation>
               <xs:documentation>Flag to indicate the change the resolution of an
						image by
						changing its pixel dimensions.
					</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:simpleContent>
                  <xs:extension base="xs:boolean">
                     <xs:attribute form="unqualified" name="Direction" type="ResampleAssetTypeCode">
                        <xs:annotation>
                           <xs:documentation> Down sampling is decreasing the number of
										pixels, and resampling up (or up sampling) is increasing the
										number.
									</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:simpleContent>
            </xs:complexType>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Resolution" nillable="true">
            <xs:complexType>
               <xs:simpleContent>
                  <xs:extension base="xs:integer">
                     <xs:attribute form="unqualified" name="Units" type="ResolutionUnitsTypeCode"/>
                  </xs:extension>
               </xs:simpleContent>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ImageDataType">
      <xs:annotation>
         <xs:documentation>Details about the image asset</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" minOccurs="0" name="ColorModel" type="ColorModelType"/>
         <xs:element form="qualified" minOccurs="0" name="CreativeDirection" type="xs:string">
            <xs:annotation>
               <xs:documentation>text note direction to photographer, stylist,
						etc.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ImageAssetSize" type="ImageAssetSizeType"/>
         <xs:element form="qualified" minOccurs="0" name="Layers" type="xs:integer">
            <xs:annotation>
               <xs:documentation>Number of layers in the image</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Logo" type="LogoType"/>
         <xs:element form="qualified" minOccurs="0" name="MediaFormat" type="MediaFormatType"/>
         <xs:element form="qualified" minOccurs="0" name="Resize" type="xs:string">
            <xs:annotation>
               <xs:documentation>Structure that provides pointer to original asset
						(if
						re-tasked)
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute form="unqualified" name="AssetFormat" type="ImageAssetFormatTypeCode" use="optional">
         <xs:annotation>
            <xs:documentation>TIFF, JPG, GIF, RTF, PNG</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="ImageFormat" type="ImageFormatCode">
         <xs:annotation>
            <xs:documentation>AAIA - PIES - Describes how the image was
					created.
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="ImageType" type="ImageTypeCode">
         <xs:annotation>
            <xs:documentation>AAIA - PIES - Image types</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="ThumbnailFlag" type="xs:boolean">
         <xs:annotation>
            <xs:documentation>Indicates reduced size image</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute default="false" form="unqualified" name="TransparencyFlag" type="xs:boolean">
         <xs:annotation>
            <xs:documentation>Indicates if the background is transparent –
					default -
					false
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="ItemIdentificationType">
      <xs:annotation>
         <xs:documentation>Identification of this item</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" minOccurs="0" name="Division" type="xs:string">
            <xs:annotation>
               <xs:documentation>N/A if UPC provided)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Restrictions" type="xs:string"/>
         <xs:element form="qualified" minOccurs="0" name="ModelNumber" type="xs:string">
            <xs:annotation>
               <xs:documentation>Manufacturers model number or stock number used
						by the
						manufacturer to identify the product
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Manufacturer" type="xs:string">
            <xs:annotation>
               <xs:documentation>Name of the manufacturer of the item, different
						from the
						brand.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ItemInAssetType">
      <xs:annotation>
         <xs:documentation>details about this item in the asset
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" minOccurs="0" name="BrandName" type="xs:string">
            <xs:annotation>
               <xs:documentation>The specific Brand or Brand Line of the item in
						the
						asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="CameraPerspective" type="CameraPerspectiveType">
            <xs:annotation>
               <xs:documentation>AAIA - PIES - Product Orientation as shown in
						Image Digital
						Assets
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="ColorCode" type="xs:string">
            <xs:annotation>
               <xs:documentation>NRF Color Code</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Description" type="DescriptionCommonData">
            <xs:annotation>
               <xs:documentation>Description of the item</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="DesignFeature" type="xs:string">
            <xs:annotation>
               <xs:documentation>Sleeveless, crew neck, short sleeve, etc.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Fabrication" type="xs:string">
            <xs:annotation>
               <xs:documentation>Linen, cotton</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Gender" type="xs:string">
            <xs:annotation>
               <xs:documentation>If any, intended gender for a gender bias product
						– men’s or
						women;s for shoes, any or both for a sweatshirt
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="IntendedUsage" type="xs:string"/>
         <xs:element form="qualified" minOccurs="0" name="IntroductionDate" type="xs:date">
            <xs:annotation>
               <xs:documentation>Month, day and year and item will first be
						publicized to the
						public.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" name="ItemID" type="ItemIDCommonData">
            <xs:annotation>
               <xs:documentation>Allows for multiple item id's for the same item,
						i.e. the SKU
						and UPC to be sent in one XML Document
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ItemIdentification" type="ItemIdentificationType"/>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Lifestyle" type="xs:string">
            <xs:annotation>
               <xs:documentation>The life style for which this particular item can
						be
						used.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="LocationInAsset" type="xs:string">
            <xs:annotation>
               <xs:documentation>Specific location that is viewable in the image
						background –
						examples could be Time Square, 7 Mile Beach, etc.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ManufacturerURL" type="xs:string">
            <xs:annotation>
               <xs:documentation>AAIA - PIES - Link to the manufacturer's
						page.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="MAPPrice" type="AmountCommonData">
            <xs:annotation>
               <xs:documentation>Minimum Advertised Price of an Item
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="MerchandiseHierarchy" type="MerchandiseHierarchyCommonData">
            <xs:annotation>
               <xs:documentation>The location in the merchandise structure
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element form="qualified" name="PriceRange">
               <xs:annotation>
                  <xs:documentation>Minimum: Maximum range of price for an item
							indicated in
							an asset
						</xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:simpleContent>
                     <xs:extension base="AmountCommonData">
                        <xs:attribute default="true" form="unqualified" name="MaximumFlag" type="xs:boolean">
                           <xs:annotation>
                              <xs:documentation>Minimum/Maximum Flag</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:extension>
                  </xs:simpleContent>
               </xs:complexType>
            </xs:element>
            <xs:element form="qualified" name="PricePoint">
               <xs:annotation>
                  <xs:documentation>The predefined list of price points between two
							actors –
							such as value, premium, etc.
						</xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:simpleContent>
                     <xs:extension base="AmountCommonData">
                        <xs:attribute default="true" form="unqualified" name="MaximumFlag" type="xs:boolean">
                           <xs:annotation>
                              <xs:documentation>Minimum/MaximumFlag</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:extension>
                  </xs:simpleContent>
               </xs:complexType>
            </xs:element>
         </xs:choice>
         <xs:element form="qualified" minOccurs="0" name="Sizing" type="xs:string">
            <xs:annotation>
               <xs:documentation>Petite, maternity</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="SuggestedRetailPrice" type="PriceCommonData">
            <xs:annotation>
               <xs:documentation>the price of the merchandise at retail
						recommended by the
						manufacturer to the retailer
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ViewDescription" type="xs:string">
            <xs:annotation>
               <xs:documentation>Standard cinematic description used in
						storyboarding or art
						direction as a general indication of the view
						– examples Close Up, Modem,
						Right Angle, Left Angle, High Angle,
						Down View, Wide Shot.
						Combinations of
						these are possible, such as
						Right Angle Medium Shot.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute form="unqualified" name="CountryOfOrigin" type="CountryCode">
         <xs:annotation>
            <xs:documentation>Country where the item is originally
					manufactured.
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute default="false" form="unqualified" name="GiftWrapAvailableFlag" type="xs:boolean">
         <xs:annotation>
            <xs:documentation>Indicates gift wrapping is available for this
					item
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="Mounted" type="MountedTypeCode">
         <xs:annotation>
            <xs:documentation>AAIA - PIES - Identifies if the item is shown
					mounted in the
					containing assembly
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="InPackage">
         <xs:annotation>
            <xs:documentation>AAIA - PIES - the item is show in or out of its
					package.
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="LogoType">
      <xs:sequence>
         <xs:element form="qualified" name="FileName" type="xs:string"/>
      </xs:sequence>
      <xs:attribute form="unqualified" name="LogoFormat" type="xs:string" use="optional">
         <xs:annotation>
            <xs:documentation>pdf, eps, Illustrator vs. Photoshop (State which
					one)
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="MediaFormatType">
      <xs:annotation>
         <xs:documentation>Technical description of asset</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" name="Format" type="xs:string">
            <xs:annotation>
               <xs:documentation>jpg, gif, tif, eps, (new: PNG)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="Lossless" type="xs:string">
            <xs:annotation>
               <xs:documentation>Lossless vs. Lossy format</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="ThreeD" type="xs:string">
            <xs:annotation>
               <xs:documentation>Format of digital asset for three dimensional
						representation,
						such as vector, cad, etc.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="OriginOfTheAssetType">
      <xs:annotation>
         <xs:documentation>Location where the content originated
				(manufacturer,
				photographer)
			</xs:documentation>
      </xs:annotation>
      <xs:simpleContent>
         <xs:extension base="xs:string">
            <xs:attribute form="unqualified" name="CountryOfOrigin" type="CountryCode">
               <xs:annotation>
                  <xs:documentation>The country where this version of this asset
							originated
						</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
   <xs:complexType name="PartyType">
      <xs:annotation>
         <xs:documentation>Information about the parties involved with this
				asset
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" name="OwnershipData" type="xs:string">
            <xs:annotation>
               <xs:documentation>DRM information (out of scope for this
						version)
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" name="Talent" type="TalentType"/>
      </xs:sequence>
      <xs:attribute form="unqualified" name="Role" type="RoleTypeCode">
         <xs:annotation>
            <xs:documentation>Creator, OriginalOwner, ContentOwner,
					CopyrightOwner,
					Photographer, BrokerAgent, Talent, Source
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:complexType name="PCMMediaType">
      <xs:annotation>
         <xs:documentation>IThe location where the other industry standard
				meta data is
				placed.
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" minOccurs="0" name="MPEG7">
            <xs:annotation>
               <xs:documentation>The framework around the MPEG7 extensions
					</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:any namespace="##other"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="DIG35">
            <xs:annotation>
               <xs:documentation>The framework around the DIG 35 extensions
					</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:any namespace="##other"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="AssetPayload" type="xs:base64Binary">
            <xs:annotation>
               <xs:documentation>The physical asset if attached to this XML
						document
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="AspectType">
      <xs:simpleContent>
         <xs:extension base="DecimalCommonData">
            <xs:attribute default="INH" form="unqualified" name="UnitOfMeasureCode" type="UnitOfMeasureCodeCommonData"/>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
   <xs:complexType name="PriceType">
      <xs:annotation>
         <xs:documentation>Price for this asset</xs:documentation>
      </xs:annotation>
      <xs:simpleContent>
         <xs:extension base="AmountCommonData">
            <xs:attribute form="unqualified" name="InternalFlag" type="xs:boolean">
               <xs:annotation>
                  <xs:documentation>No – External Yes – Internal</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
   <xs:complexType name="ProviderListType">
      <xs:annotation>
         <xs:documentation>List of providers for this asset</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" maxOccurs="unbounded" name="Provider" type="xs:string">
            <xs:annotation>
               <xs:documentation>The name of the provider or potential provider
						for this
						asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ReplacementForType">
      <xs:annotation>
         <xs:documentation>New Content intended to replace existing asset
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="OriginalUUID" type="UUIDType">
            <xs:annotation>
               <xs:documentation>UUID for the asset being replaced
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" name="ReplacementUUID" type="UUIDType">
            <xs:annotation>
               <xs:documentation>UUID for the replacement asset</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" name="Reason" type="xs:string">
            <xs:annotation>
               <xs:documentation>Reason the asset is being replaced
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ResolutionType">
      <xs:annotation>
         <xs:documentation>AAIA - PIES - DPI of Digital Asset – 72 / 96 / 300
				/ 600 / 800 /
				1200
			</xs:documentation>
      </xs:annotation>
      <xs:simpleContent>
         <xs:extension base="ResolutionFixedTypeCode">
            <xs:attribute form="unqualified" name="Units" type="ResolutionUnitsTypeCode"/>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
   <xs:complexType name="TalentType">
      <xs:annotation>
         <xs:documentation>Information about the talent in the asset
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" name="Name" type="NameCommonData">
            <xs:annotation>
               <xs:documentation>Name of actor or model used for creation of an
						asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" name="Description" type="xs:string">
            <xs:annotation>
               <xs:documentation>Specific identification of the talent in an
						image. For
						example, may identify a celebrity as the official
						spokesperson for a brand
						or product included in the asset.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="StartDate" type="xs:date">
            <xs:annotation>
               <xs:documentation>Month day and year that the talent release
						authorizes use of
						the asset.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="EndDate" type="xs:date">
            <xs:annotation>
               <xs:documentation>Month day and year that the talent release
						extends to
						authorized use of the asset per the fee paid the talent.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" minOccurs="0" name="MediaUsageType" type="xs:string">
            <xs:annotation>
               <xs:documentation>typically, a talent release authorizes specific
						use of an
						image, such as for print (which then excludes web use,
						etc.) internal
						use,
						external use, etc.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="SellingAssociationType">
      <xs:annotation>
         <xs:documentation>Identifies a selling relationship between two PCM
				Assets
			</xs:documentation>
      </xs:annotation>
      <xs:simpleContent>
         <xs:extension base="UUIDType">
            <xs:attribute default="UpSell" form="unqualified" name="AssociationType" type="SellingAssociationTypeCode">
               <xs:annotation>
                  <xs:documentation>UpSell, CrossSell, etc.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute default="Child" form="unqualified" name="Relationship" type="SellingAssociationRelationshipTypeCode">
               <xs:annotation>
                  <xs:documentation>Parent, Child, Sibling</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
   <xs:complexType name="UnstructuredDataType">
      <xs:annotation>
         <xs:documentation>Details about the Unstructured asset
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" minOccurs="0" name="AssetSize" type="DecimalCommonData"/>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Language" type="LanguageCode"/>
         <xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
      </xs:sequence>
      <xs:attribute form="unqualified" name="AssetFormat" type="xs:string"/>
   </xs:complexType>
   <xs:complexType name="UsageRestrictionType">
      <xs:annotation>
         <xs:documentation>Information about how this asset can be used (e.g.
				dates of use,
				copyright, digital asset usage, model rights, related
				text or digital assets,
				approval/review status, redistribution) Use
				Restriction likely needs
				to include
				concept of mandated frequency of
				check for updates
			</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element form="qualified" maxOccurs="unbounded" name="EntitledParty" type="EntitledPartyType">
            <xs:annotation>
               <xs:documentation>Information about this party's access to this
						asset
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="EntitledLocation" type="EntitledLocationType"/>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Payment">
            <xs:annotation>
               <xs:documentation>Fees owed to the individual parties
					</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:simpleContent>
                  <xs:extension base="AmountCommonData">
                     <xs:attribute form="unqualified" name="Role" type="RoleTypeCode"/>
                  </xs:extension>
               </xs:simpleContent>
            </xs:complexType>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="PromotionEvent" type="xs:string"/>
         <xs:element form="qualified" name="RestrictionDocument" type="xs:string">
            <xs:annotation>
               <xs:documentation>A reference to an external document (such as an
						electronic use
						agreement between two parties) that more
						specifically defines how assets
						may
						be used.
					</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="SeasonData" type="xs:string"/>
         <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="UseFor" type="xs:string">
            <xs:annotation>
               <xs:documentation>Potential uses for this asset - Web, Collateral,
						in-store,
						POP, Billboard, retailer
					</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute form="unqualified" name="MediaType" type="MediaTypeCode">
         <xs:annotation>
            <xs:documentation>Specifically defines the medium where that asset
					may be used, such
					as printed catalog, online web, point-of-sale,
					etc.
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="InternalUseOnlyFlag" type="xs:boolean">
         <xs:annotation>
            <xs:documentation>As it pertains to retailers, specifically
					indicates that an asset
					can be used by any person within an
					organization for internal purposes
					but
					cannot be used in any customer
					or public facing media.
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute form="unqualified" name="TalentReleaseFlag" type="xs:boolean">
         <xs:annotation>
            <xs:documentation>If an asset contains the image of recognizable
					person, then the
					provider is photography that a talent release has
					been secured for the
					usage
					restrictions granted in this message.
				</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
