<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns0="http://www.agarcia.mx/SAALMA/Ajustepiezasrecibidas" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="AjusteDePiezasRecibidasRestReference">RestReference</doc>
   <grammars>
      <xsd:schema>
            <xsd:import namespace="http://www.agarcia.mx/SAALMA/Ajustepiezasrecibidas" schemaLocation="../Schemas/DeliveryQuantityAdjustmentRequest.xsd"/>
        </xsd:schema>
      <xsd:schema>
            <xsd:import namespace="http://www.agarcia.mx/SAALMA/Ajustepiezasrecibidas" schemaLocation="../Schemas/DeliveryQuantityAdjustmentResponse.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/Ajustepiezasrecibidas">
         <method name="POST" soa:wsdlOperation="POST">
            <doc title="POST POST">Permite a SAALMA el envío de una solicitud para subir el Ajuste de Piezas Recibidas por día.</doc>
            <request>
               <representation mediaType="application/json" element="cns:DeliveryQuantityAdjustmentRequest" xmlns:cns="http://www.agarcia.mx/SAALMA/Ajustepiezasrecibidas"/>
            </request>
            <response status="200">
               <representation mediaType="application/json" element="cns:DeliveryQuantityAdjustmentResponse" xmlns:cns="http://www.agarcia.mx/SAALMA/Ajustepiezasrecibidas"/>
            </response>
         </method>
      </resource>
   </resources>
</application>