<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:inp1="http://http://mx.agarcia.ea/Applications/Governance/GovernanceAPI" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="GovernanceAPIRestPL">GovernanceAPI</doc>
   <grammars>
      <xsd:schema xmlns:tns="http://xmlns.oracle.com/GovernanceAPIApp/GovernanceAPI/GovernanceAPIBS" xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">
            <xsd:import namespace="http://http://mx.agarcia.ea/Applications/Governance/GovernanceAPI" schemaLocation="../../../GovernanceAPI/Resources/NXSD/GovernanceAPI.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/sendUpdate">
         <method name="PUT" soa:wsdlOperation="sendUpdate">
            <request>
               <representation mediaType="application/xml" element="cns:request" xmlns:cns="http://http://mx.agarcia.ea/Applications/Governance/GovernanceAPI"/>
               <representation mediaType="application/json" element="cns:request" xmlns:cns="http://http://mx.agarcia.ea/Applications/Governance/GovernanceAPI"/>
            </request>
            <response status="204"/>
         </method>
      </resource>
      <resource path="/send">
         <method name="POST" soa:wsdlOperation="send">
            <request>
               <representation mediaType="application/xml" element="cns:request" xmlns:cns="http://http://mx.agarcia.ea/Applications/Governance/GovernanceAPI"/>
               <representation mediaType="application/json" element="cns:request" xmlns:cns="http://http://mx.agarcia.ea/Applications/Governance/GovernanceAPI"/>
            </request>
            <response status="204"/>
         </method>
      </resource>
   </resources>
</application>