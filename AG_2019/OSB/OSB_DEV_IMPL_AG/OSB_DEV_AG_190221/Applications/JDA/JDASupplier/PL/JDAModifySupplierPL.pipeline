    <Parameters id="supplier.update">
        <Parameter name="ASNUM">{$body/mod:modifySupplierRequest/mod:supplier/v11:supplierID/text()}</Parameter>
        <Parameter name="ASNAME">{$body/mod:modifySupplierRequest/mod:supplier/v11:contactName/text()}</Parameter>
