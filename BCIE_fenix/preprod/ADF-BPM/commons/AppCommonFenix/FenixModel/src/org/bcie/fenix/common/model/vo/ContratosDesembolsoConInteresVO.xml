  <SQLQuery><![CDATA[SELECT CD.ID,           
CD.ID_TCA_ESTADO,           
TSL.ID_LINEA,                       
CD.MONTO_DESEMBOLSAR_USD AS MONTO,       
CD.MONTO_DESEMBOLSAR AS MONTO_DESEMBOLSAR,       
'' INTERES,                  
'' FECHA_VIGENCIA,                
CD.CONTRATO_FLEXCUBE                       
FROM CONTRATO_DESEMBOLSO CD                       
INNER JOIN TRE_SOLICITUD_LINEA_CREDITO TSL ON TSL.ID_CONTRATO_DESEMBOLSO = CD.ID             
INNER JOIN LINEA_CREDITO ON LINEA_CREDITO.ID = TSL.ID_LINEA             
INNER JOIN CONTRATO ON CONTRATO.ID = LINEA_CREDITO.ID_CONTRATO        
WHERE CD.BAN_ESTATUS=1  
AND CONTRATO.ID_OPERACION =:pIdOperacion  
AND (CD.ID_TCA_ESTADO=:pIdEstadoContrato  
      OR CD.ID_TCA_ESTADO=:pEstadoCreado  
      OR CD.ID_TCA_ESTADO=:pEstadoValidado 
      OR CD.ID_TCA_ESTADO=:pEstadoLiquidado)  
ORDER BY CD.ID ASC]]></SQLQuery>
