  <SQLQuery><![CDATA[SELECT      
    LC.ID,     
    LC.ID_CONTRATO,     
    LC.ID_TCA_TIPO_MORA,     
    LC.SPREAD_TASA,     
    LC.VALOR_TASA,     
    LC.CODIGO_TASA_REFERENCIA,     
    LC.FRECUENCIA,     
    LC.ID_TCA_TIPO_FRECUENCIA,     
    LC.DIAS_SPOT_TASA,     
    LC.ID_TCA_TIPO_REDONDEO,     
    LC.CANTIDAD_DECIMALES,     
    TER.TASA_MINIMA_DESEMBOLSO,      
    TER.TASA_MAXIMA_DESEMBOLSO     
FROM LINEA_CREDITO LC     
JOIN TRE_LINEA_CREDITO_TCC TLC ON LC.ID=TLC.ID_LINEA_CREDITO     
JOIN TERMINO TER ON TLC.ID_TERMINO=TER.ID     
JOIN TCA_TERMINO TT ON TT.ID = TER.ID_TCA_TERMINO     
WHERE TT.DESCRIPCION_CORTA = 'T501' AND LC.ID=:ID_LINEA]]></SQLQuery>
