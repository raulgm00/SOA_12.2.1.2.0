                                                        shortDesc="#{bindings.OperacionResultadoVO.hints.Etapa.tooltip}"
                                                        readOnly="true" id="soc15">
                                        <f:selectItems value="#{row.bindings.Etapa.items}" id="si20"/>
                                        <f:validator binding="#{row.bindings.Etapa.validator}"/>
                                    </af:selectOneChoice>
                                </af:column>
                                <af:column sortProperty="#{bindings.OperacionResultadoVO.hints.Estado.name}"
                                           sortable="true" headerText="#{gestoroperacionesfenixvcBundle.ESTADO_SP}"
