                                        <f:validator binding="#{row.bindings.Nombrepais.validator}"/>
                                    </af:selectOneChoice>
                                </af:column>
                                <af:column sortProperty="#{bindings.OperacionResultadoVO.hints.Nombreproducto.name}"
