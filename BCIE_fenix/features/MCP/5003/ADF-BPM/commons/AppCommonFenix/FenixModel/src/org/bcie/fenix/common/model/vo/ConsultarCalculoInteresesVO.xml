  <Variable
    Name="varIdPrepago"
    Kind="where"
    Type="java.lang.Long"/>
  <SQLQuery><![CDATA[SELECT PREPAGO.ID AS ID_PREPAGO,                  
       PREPAGO.ID_TCA_TIPO_RESOLUCION,                
       PREPAGO.FECHA_PREPAGO,        
       TRE_PREPAGO_CONTRATO.ID AS ID_TRE_PREPAGO_CONTRATO,                       
       CALCULO_INTERESES.ID AS ID_CALCULO_INTERESES,                      
       TRE_PREPAGO_CONTRATO.CONTRATO_FLEXCUBE,                      
       CALCULO_INTERESES.BASE,                      
       CALCULO_INTERESES.TASA,                      
       TRE_PREPAGO_CONTRATO.MONTO_PREPAGO,                      
       CALCULO_INTERESES.DESDE,                      
       CALCULO_INTERESES.HASTA,                      
       CALCULO_INTERESES.DIAS,                      
       CALCULO_INTERESES.INTERESES,                      
       TRE_PREPAGO_CONTRATO.FECHA_PROXIMO_PAGO,                     
       TCA_TIPO_MONEDA.DESCRIPCION_CORTA,                  
       TRE_PREPAGO_CONTRATO.CAPITAL_NO_VENCIDO        
FROM PREPAGO                       
INNER JOIN TRE_PREPAGO_CONTRATO                       
  ON PREPAGO.ID = TRE_PREPAGO_CONTRATO.ID_PREPAGO                       
INNER JOIN CALCULO_INTERESES                       
  ON TRE_PREPAGO_CONTRATO.ID = CALCULO_INTERESES.ID_TRE_PREPAGO_CONTRATO                       
INNER JOIN TCA_TIPO_MONEDA                       
  ON TRE_PREPAGO_CONTRATO.ID_TCA_TIPO_MONEDA = TCA_TIPO_MONEDA.ID                       
WHERE         
     PREPAGO.ID = :varIdPrepago          
  AND  
       PREPAGO.FECHA_PREPAGO <> TRE_PREPAGO_CONTRATO.FECHA_PROXIMO_PAGO 
    AND 
        TRE_PREPAGO_CONTRATO.MONTO_PREPAGO = TRE_PREPAGO_CONTRATO.CAPITAL_NO_VENCIDO 
        AND 
            (PREPAGO.ID_TCA_TIPO_RESOLUCION = 1     
        OR      
          PREPAGO.ID_TCA_TIPO_RESOLUCION = 3)]]></SQLQuery>
