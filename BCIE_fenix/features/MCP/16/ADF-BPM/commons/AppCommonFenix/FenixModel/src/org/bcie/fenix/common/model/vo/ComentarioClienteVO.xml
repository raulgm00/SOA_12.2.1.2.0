  <SQLQuery><![CDATA[SELECT ComentarioClienteEO.ID,             
       ComentarioClienteEO.COMENTARIO,             
       TO_CHAR(ComentarioClienteEO.FECHA_REGISTRO, 'YYYY-MM-DD HH24:MI:SS') FECHA_REGISTRO,                   
       ComentarioClienteEO.ID_INSTANCIA_TAREA,             
       ComentarioClienteEO.ID_CLIENTE,             
       ComentarioClienteEO.ID_TCA_TAREA,             
       ComentarioClienteEO.LOGIN_USUARIO,             
       ComentarioClienteEO.NOMBRE_USUARIO,          
       ComentarioClienteEO.BAN_ESTATUS,        
       (SELECT DESCRIPCION FROM TCA_TAREA_BPM WHERE ID = ComentarioClienteEO.ID_TCA_TAREA) AS NombreTarea,                
       (SELECT DESCRIPCION FROM TCA_PROCESO_BPM WHERE ID =(select ID_PROCESO_BPM  from TCA_TAREA_BPM where ID = ComentarioClienteEO.ID_TCA_TAREA )) AS NombreProceso                
FROM  COMENTARIO_CLIENTE ComentarioClienteEO              
WHERE ComentarioClienteEO.ID_CLIENTE = :varIdCliente AND         
ComentarioClienteEO.BAN_ESTATUS = 1         
ORDER BY ComentarioClienteEO.ID DESC]]></SQLQuery>
