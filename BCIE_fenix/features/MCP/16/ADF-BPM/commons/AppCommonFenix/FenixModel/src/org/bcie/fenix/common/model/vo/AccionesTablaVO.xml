  <SQLQuery><![CDATA[SELECT acc.ID ID,                         
       acc.ID_CLIENTE ID_CLIENTE,                         
       acc.ID_OPERACION ID_OPERACION,                         
       acc.ACCION ACCION,                         
       acc.ID_ESTADO_ACCION ID_ESTADO_ACCION,          
       acc.ID_ESTADO_ACCION ID_ESTADO_ACCION2,        
       acc.ID_TCA_CATEGORIA_ACCION ID_TCA_CATEGORIA_ACCION,                         
       acc.ID_TCA_ROL_ASIGNADO ID_TCA_ROL_ASIGNADO,                         
       acc.FECHA_REGISTRO FECHA_REGISTRO,               
       acc.FECHA_ATENCION FECHA_ATENCION,                
       acc.BAN_ESTATUS BAN_ESTATUS,                
       trb.DESCRIPCION_CORTA NOMBRE_ROL_ASIGNADO,               
       tea.DESCRIPCION NOMBRE_ESTADO_ACCION  
FROM ACCION acc,  
     TCA_ROL_BPM trb,  
     TCA_ESTADO_ACCION tea  
WHERE 1 = 1  
  
  --ACCION VS TCA_ROL_BPM  
  AND acc.ID_TCA_ROL_ASIGNADO = trb.ID  
    
  --ACCION VS TCA_ESTADO_ACCION  
  AND acc.ID_ESTADO_ACCION = tea.ID  
    
  AND(  
  (acc.ID_OPERACION = :pIdOperacion  AND acc.ID_CLIENTE IS NULL) OR                   
  (acc.ID_CLIENTE = :pIdCliente AND acc.ID_OPERACION IS NULL)  
  )  
   ORDER BY acc.ID_ESTADO_ACCION ASC]]></SQLQuery>
