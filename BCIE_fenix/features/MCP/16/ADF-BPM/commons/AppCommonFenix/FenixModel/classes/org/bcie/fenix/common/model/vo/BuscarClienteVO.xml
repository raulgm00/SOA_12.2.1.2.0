  <SQLQuery><![CDATA[SELECT             
    CLIENTES.RAZON_SOCIAL RAZON_SOCIAL,         
    CAT_PAISES.DESCRIPCION_CORTA pais,   
    CLIENTES.ID_CLIENTE,            
    CLIENTES.ABREVIATURA ABREVIATURA,         
    CAT_TIPO_CLIENTE.ID tipoClienteId,         
    CLIENTES.NUMERO_IDENTIFICACION NUMERO_IDENTIFICACION,         
    CLIENTES.ID_FLEXCUBE ID_FLEXCUBE,         
    CLIENTES.EJECUTIVO EJECUTIVO,   
    CAT_PAISES.ID AS paisId,        
    CAT_TIPO_CLIENTE.DESCRIPCION_CORTA tipoCliente,   
    CAT_SECTOR_INSTITUCIONAL.DESCRIPCION_CORTA Sector,         
    CAT_SECTOR_INSTITUCIONAL.ID AS sectorId,        
    CAT_TIPO_PERSONA.DESCRIPCION AS tipoPersona,         
    CAT_TIPO_PERSONA.ID AS tipoPersonaId,        
    CLIENTES.DIA_PAGO,          
    CAT_OFICINA.DESCRIPCION AS Oficina,         
    CAT_OFICINA.ID AS oficinaId,        
    CAT_TIPO_INSTITUCION.DESCRIPCION AS tipoInstitucion,          
    CAT_TIPO_INSTITUCION.ID AS tipoInstitucionId,        
    CAT_GRUPO_ECONOMICO.DESCRIPCION AS grupoEconomico,        
    CAT_GRUPO_ECONOMICO.ID AS grupoEconomicoId        
FROM                  
    CLIENTES                  
LEFT JOIN CAT_PAISES ON CLIENTES.PAIS = CAT_PAISES.ID                  
LEFT JOIN CAT_TIPO_CLIENTE ON CLIENTES.TIPO_CLIENTE = CAT_TIPO_CLIENTE. ID               
LEFT JOIN CAT_SECTOR_INSTITUCIONAL ON CLIENTES.SECTOR = CAT_SECTOR_INSTITUCIONAL.ID         
LEFT JOIN CAT_TIPO_PERSONA ON CLIENTES.TIPO_PERSONA = CAT_TIPO_PERSONA.ID         
LEFT JOIN CAT_OFICINA ON CLIENTES.OFICINA = CAT_OFICINA.id         
LEFT JOIN CAT_TIPO_INSTITUCION ON CLIENTES.TIPO_INSTITUCION = CAT_TIPO_INSTITUCION.ID         
LEFT JOIN CAT_GRUPO_ECONOMICO ON CLIENTES.GRUPO_ECONOMICO = CAT_GRUPO_ECONOMICO.ID  
WHERE CLIENTES.BAN_ESTATUS = '1']]></SQLQuery>
