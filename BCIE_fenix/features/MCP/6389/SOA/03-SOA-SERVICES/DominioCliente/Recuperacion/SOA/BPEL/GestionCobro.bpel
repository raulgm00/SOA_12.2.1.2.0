            </documentation>
            <sequence name="SequenceSinDatos">
              <assign name="AssignSinDatos">
                <copy>
                  <from>'OK'</from>
                  <to>$outputVariable.response/ns1:Resultado/ns2:result</to>
                </copy>
                <copy>
