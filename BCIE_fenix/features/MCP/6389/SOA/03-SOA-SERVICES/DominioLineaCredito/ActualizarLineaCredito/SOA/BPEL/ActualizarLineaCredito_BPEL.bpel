                        xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"/>
              </sequence>
            </scope>
          </forEach>
          <else>
            <documentation>
              <![CDATA[ERROR]]>
            </documentation>
            <sequence name="SequenceActualizarSoloActual">
              <assign name="ActualizarContratoSequence">
                <copy>
