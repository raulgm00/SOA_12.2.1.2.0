          <documentation>
            <![CDATA[OK]]>
          </documentation>
          <condition>$outConsultarContrato.response/ns13:Resultado/ns16:result = 'OK'</condition>
          <forEach parallel="no" counterName="CounterContratos" name="ForEachContratos">
