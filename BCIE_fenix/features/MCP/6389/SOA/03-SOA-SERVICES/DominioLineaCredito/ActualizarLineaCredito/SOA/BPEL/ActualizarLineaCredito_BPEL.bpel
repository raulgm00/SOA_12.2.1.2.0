                </copy>
                <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                  <from>$inputVariable.request/ns13:Contrato/ns7:fechaValor</from>
                  <to>$inActualizarContrato.request/ns6:Contrato/ns7:fechaValor</to>
