                    </copy>
                  </assign>
                  <assign name="XQ_RequestCrearActualizarDetalleTransaccion">
                    <bpelx:annotation>
                      <bpelx:pattern patternName="bpelx:xquery"></bpelx:pattern>
                    </bpelx:annotation>
                    <copy>
