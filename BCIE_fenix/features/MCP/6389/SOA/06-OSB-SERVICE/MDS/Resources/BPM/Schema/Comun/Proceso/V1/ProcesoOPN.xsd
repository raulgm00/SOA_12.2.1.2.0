           xmlns:proceso="http://xmlns.bcie.org/ObjetoProceso/Comun/Proceso/V1" 
           elementFormDefault="qualified"
           attributeFormDefault="unqualified" version="1.0">
  
  <xs:complexType name="Proceso">
