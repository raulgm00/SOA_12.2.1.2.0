  <SQLQuery><![CDATA[SELECT CONTRATO_DESEMBOLSO.BAN_ESTATUS,                                        
       CONTRATO_DESEMBOLSO.FECHA_EFECTIVA,                                        
       CONTRATO_DESEMBOLSO.FECHA_ESTIMADA_DESEMBOLSAR,                                        
       CONTRATO_DESEMBOLSO.FECHA_INICIO_PROCESO_DESEM FECHA_INI_PROCES,                                        
       CONTRATO_DESEMBOLSO.ID,                                        
       CONTRATO_DESEMBOLSO.ID_TCA_ESTADO,                                        
       CONTRATO_DESEMBOLSO.ID_TCA_TIPO_MONEDA,                                        
       CONTRATO_DESEMBOLSO.MONTO_DESEMBOLSAR,                                        
       CONTRATO_DESEMBOLSO.PROGRAMADO,                                
       CONTRATO_DESEMBOLSO.CONTRATO_FLEXCUBE,                                
       CONTRATO_DESEMBOLSO.CUENTA_CON_RECURSOS_EXTERNOS,  
       CONTRATO_DESEMBOLSO.ID_TRANSFERENCIA_FT05,  
       TRE_SOLICITUD_LINEA_CREDITO.ID_LINEA,                          
       TRE_SOLICITUD_LINEA_CREDITO.ID_SOLICITUD,                         
       TCA_ESTADO_OPERACION.DESCRIPCION,                       
       (SELECT TTM.DESCRIPCION_CORTA FROM TCA_TIPO_MONEDA TTM WHERE TTM.ID = CONTRATO_DESEMBOLSO.ID_TCA_TIPO_MONEDA) AS TIPO_MONEDA                       
FROM  CONTRATO_DESEMBOLSO                                      
INNER JOIN TRE_SOLICITUD_LINEA_CREDITO                                      
ON CONTRATO_DESEMBOLSO.ID = TRE_SOLICITUD_LINEA_CREDITO.ID_CONTRATO_DESEMBOLSO                             
INNER JOIN TCA_ESTADO_OPERACION                             
ON TCA_ESTADO_OPERACION.ID = CONTRATO_DESEMBOLSO.ID_TCA_ESTADO                          
WHERE CONTRATO_DESEMBOLSO.BAN_ESTATUS = 1 AND CONTRATO_DESEMBOLSO.FECHA_EFECTIVA IS NOT NULL]]></SQLQuery>
