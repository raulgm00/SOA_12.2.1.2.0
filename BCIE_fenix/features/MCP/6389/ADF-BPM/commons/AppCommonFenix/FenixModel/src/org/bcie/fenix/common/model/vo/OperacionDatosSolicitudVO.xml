  <SQLQuery><![CDATA[SELECT         
    OP1.ID_OPERACION ID_OPERACION,                
    OP1.NOMBRE NOMBRE_OPERACION,                
    CLIENTES.RAZON_SOCIAL RAZON_SOCIAL_CLIENTE,                        
    OP1.USUARIO RESPONSABLE_OPERACION,                
    CLIENTES.PAIS PAIS_CLIENTE,                
    CAT_PRODUCTO.DESCRIPCION DESCRIPCION_PRODUCTO,               
    OP1.ID_PRODUCTO ID_PRODUCTO,                
    OP1.ETAPA ETAPA,                
    OP1.ESTADO ESTADO,              
    OP1.ID_CLIENTE ID_CLIENTE          
FROM                
   OPERACION OP1                           
   INNER JOIN CLIENTES ON OP1.ID_CLIENTE = CLIENTES.ID_CLIENTE               
   INNER JOIN CAT_PRODUCTO ON OP1.ID_PRODUCTO = CAT_PRODUCTO.ID         
   INNER JOIN MONTO_OPERACION MO ON OP1.ID_OPERACION = MO.ID_OPERACION        
   WHERE OP1.BAN_ESTATUS = '1'   
     AND OP1.ID_OPERACION = (         
           CASE WHEN :pNumeroLineaCredito IS NULL THEN OP1.ID_OPERACION         
           ELSE         
           (         
             SELECT DISTINCT CO.ID_OPERACION         
             FROM LINEA_CREDITO LC,         
                  CONTRATO CO         
             WHERE 1 = 1         
               AND LC.ID_CONTRATO = CO.ID         
               AND CO.ID_OPERACION = OP1.ID_OPERACION         
               AND UPPER(LC.NUMERO_LINEA_CREDITO) LIKE UPPER('%' || :pNumeroLineaCredito || '%')         
               AND LC.ID_FLEXCUBE IS NOT NULL    
           )         
           END         
         )         
            
   GROUP BY OP1.ID_OPERACION,                
            OP1.NOMBRE,                
            CLIENTES.RAZON_SOCIAL,                        
            OP1.USUARIO,                
            CLIENTES.PAIS,                
            CAT_PRODUCTO.DESCRIPCION,               
            OP1.ID_PRODUCTO,                
            OP1.ETAPA,                
            OP1.ESTADO,                       
            OP1.ID_CLIENTE]]></SQLQuery>
