  <SQLQuery><![CDATA[SELECT DISTINCT LC.ID,         
       LC.BAN_ESTATUS,                 
       LC.CODIGO_ASIGNACION,                 
       LC.CODIGO_PLANTILLA_LIMITE,                 
       LC.CODIGO_SERIAL_LIMITE,                 
       LC.CONDICION_ESPECIAL,                 
       LC.DESCRIPCION_ASIGNACION,                 
       LC.DESCRIPCION_COND_ESPECIAL,                 
       LC.DESCRIPCION_LINEA,                 
       LC.ES_REVOLVENTE,                 
       LC.FECHA_REGISTRO,                 
       LC.FECHA_VALOR,                 
       LC.FECHA_VENCIMIENTO,                 
       LC.FONDO,            
       (SELECT CODE_DESC FROM VCA_FONDO WHERE VCA_FONDO.ACTIVE='A'              
        AND MIS_CODE = LC.FONDO AND ROWNUM < 2) AS DESCRIPCION_FONDO,          
       LC.ID_CONTRATO,                 
       LC.ID_FLEXCUBE,                 
       LC.ID_FLEXCUBE_PASIVO,                 
       LC.ID_PRODUCTO_FLEXCUBE,                 
       LC.MONTO_LINEA,                 
       LC.NUMERO_LINEA_CREDITO,                 
       LC.TRATAMIENTO_DIAS_FERIADOS,         
       TRE_SOLICITUD_LINEA_CREDITO.ID_SOLICITUD,   
       SOLICITUD_DESEMBOLSO.ID_OPERACION   
FROM  LINEA_CREDITO LC            
INNER JOIN TRE_SOLICITUD_LINEA_CREDITO ON         
LC.ID = TRE_SOLICITUD_LINEA_CREDITO.ID_LINEA   
INNER JOIN SOLICITUD_DESEMBOLSO   
ON SOLICITUD_DESEMBOLSO.ID = TRE_SOLICITUD_LINEA_CREDITO.ID_SOLICITUD]]></SQLQuery>
