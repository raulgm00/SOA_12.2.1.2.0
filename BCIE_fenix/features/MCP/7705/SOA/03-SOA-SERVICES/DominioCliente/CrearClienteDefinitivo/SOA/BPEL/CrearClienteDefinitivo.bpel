                    <to>$outputVariable.response/ns4:Resultado/ns7:result</to>
                  </copy>
                  <copy>
                    <from>'Consulta de Cliente sin resultados'</from>
                    <to>$outputVariable.response/ns4:Resultado/ns7:message</to>
                  </copy>
                </assign>
