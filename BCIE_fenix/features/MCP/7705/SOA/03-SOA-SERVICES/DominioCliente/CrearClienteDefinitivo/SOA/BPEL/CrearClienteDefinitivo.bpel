          <documentation>
            <![CDATA[ConsultaERROR]]>
          </documentation>
          <condition>$consultarByIdCliente_OutputVariable.response/ns4:Resultado/ns7:result = 'ERROR'</condition>
          <sequence name="Sequence4">
             <if name="IfConsultaVacia">
              <documentation>
                <![CDATA[Vacia]]>
              </documentation>
