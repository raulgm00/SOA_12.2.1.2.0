<?xml version="1.0" encoding="windows-1252" ?><adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">  <task-flow-definition id="liquidarContrato">    <default-activity>CargarLiquidarContrato</default-activity>    <transaction>      <requires-transaction/>    </transaction>    <data-control-scope>      <shared/>    </data-control-scope>    <input-parameter-definition id="__1">      <name>pIdDesembolso</name>      <value>#{pageFlowScope.pIdDesembolso}</value>      <class>java.lang.Long</class>      <required/>    </input-parameter-definition>    <input-parameter-definition id="__3">      <name>pIdSolicitud</name>      <value>#{pageFlowScope.pIdSolicitud}</value>      <class>java.lang.Long</class>    </input-parameter-definition>    <input-parameter-definition id="__4">      <name>pModoEjecucion</name>      <value>#{pageFlowScope.pModoEjecucion}</value>      <class>java.lang.Integer</class>      <required/>    </input-parameter-definition>    <managed-bean id="__2">      <managed-bean-name>LiquidarContratoManagedBean</managed-bean-name>      <managed-bean-class>org.bcie.fenix.view.gestordesembolsos.LiquidarContratoBean</managed-bean-class>      <managed-bean-scope>pageFlow</managed-bean-scope>      <!--oracle-jdev-comment:managed-bean-jsp-link:1WEB-INF/org/bcie/fenix/view/gestorDesembolsos/liquidarcontrato/LiquidarContrato.jsff-->    </managed-bean>    <managed-bean id="__5">      <managed-bean-name>LiquidarContratoActionBean</managed-bean-name>      <managed-bean-class>org.bcie.fenix.view.gestordesembolsos.LiquidarContratoActionBean</managed-bean-class>      <managed-bean-scope>request</managed-bean-scope>    </managed-bean>    <managed-bean id="__6">      <managed-bean-name>LiquidarContratoActionBean</managed-bean-name>      <managed-bean-class>org.bcie.fenix.view.gestordesembolsos.LiquidarContratoActionBean</managed-bean-class>      <managed-bean-scope>request</managed-bean-scope>    </managed-bean>    <method-call id="CargarLiquidarContrato">      <method>#{pageFlowScope.LiquidarContratoManagedBean.cargarLiquidarContrato}</method>      <outcome>        <fixed-outcome>*irLiquidarContrato</fixed-outcome>      </outcome>    </method-call>    <view id="LiquidarContratos">      <page>/WEB-INF/org/bcie/fenix/view/gestorDesembolsos/liquidarcontrato/LiquidarContrato.jsff</page>    </view>    <control-flow-rule id="__8">      <from-activity-id>CargarLiquidarContrato</from-activity-id>      <control-flow-case id="__9">        <from-outcome>*irLiquidarContrato</from-outcome>        <to-activity-id>LiquidarContratos</to-activity-id>      </control-flow-case>    </control-flow-rule>    <use-page-fragments/>  </task-flow-definition></adfc-config>