  <SQLQuery><![CDATA[SELECT SEGAC2.ID,       
SEGAC2.ID_ACCION,       
SEGAC2.ID_TCA_TAREA_BPM,       
SEGAC2.ID_TCA_ROL_BPM,       
 SEGAC2.ID_TCA_ESTADO_ANTERIOR,       
 SEGAC2.ID_TCA_ESTADO_DESPUES,       
 SEGAC2.LOGIN_USUARIO,       
 SEGAC2.NOMBRE_USUARIO,       
 SEGAC2.ID_OBSERVACION_ACCION,       
 SEGAC2.FECHA_REGISTRO,       
 SEGAC2.BAN_ESTATUS, 
 ACCION.FECHA_REGISTRO AS FECHA_REGISTRO_ACCION 
FROM SEGUIMIENTO_ACCION SEGAC2  
INNER JOIN ACCION ACCION ON ACCION.ID=SEGAC2.ID_ACCION 
WHERE 1 = 1       
AND (SEGAC2.ID_TCA_ESTADO_DESPUES, SEGAC2.ID) IN       
(       
 SELECT SEGAC.ID_TCA_ESTADO_DESPUES,       
MAX(SEGAC.ID)       
 FROM SEGUIMIENTO_ACCION SEGAC       
 WHERE 1 = 1       
 AND SEGAC.ID_ACCION = SEGAC2.ID_ACCION      
 GROUP BY SEGAC.ID_TCA_ESTADO_DESPUES, SEGAC.ID_ACCION      
)      
ORDER BY ID_ACCION, ID DESC]]></SQLQuery>
