    Type="java.lang.Long"/>
  <SQLQuery><![CDATA[SELECT CON.ID,           
       CON.NOMBRE,                                 
       CON.DESCRIPCION,                                 
       CON.FECHA_VIGENCIA,                              
       CON.FECHA_INICIO,                              
       CON.FECHA_FINAL,                              
       CON.PLAZO,                              
       CON.ID_TCA_FRECUENCIA_PLAZO,                            
       CON.ID_TCA_CONTROL_CONDICION,    
       CON.ID_TCA_TIPO_FECHA_INICIO,    
       MAX(VA.FECHA_REGISTRO) AS FECHA_VALIDACION           
FROM CONDICION CON                            
LEFT JOIN VALIDACION_CONDICION VA ON VA.ID_CONDICION=CON.ID                            
WHERE CON.ID_OPERACION=:pIdOperacion           
      AND CON.BAN_ESTATUS = 1         
      AND CON.ID_TCA_CONTROL_CONDICION = 2           
      AND (VA.ES_VALIDADOR = 1 OR VA.ES_VALIDADOR IS NULL)           
GROUP BY CON.ID,                                 
       CON.NOMBRE,                                 
       CON.DESCRIPCION,                                 
       CON.FECHA_VIGENCIA,                              
       CON.FECHA_INICIO,                              
       CON.FECHA_FINAL,                              
       CON.PLAZO,                              
       CON.ID_TCA_FRECUENCIA_PLAZO,                            
       CON.ID_TCA_CONTROL_CONDICION,    
       CON.ID_TCA_TIPO_FECHA_INICIO]]></SQLQuery>
