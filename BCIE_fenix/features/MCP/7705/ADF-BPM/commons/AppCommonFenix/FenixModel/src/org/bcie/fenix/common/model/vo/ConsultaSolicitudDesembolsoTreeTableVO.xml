  <SQLQuery><![CDATA[SELECT QRSLTSQ1.* FROM (   
SELECT                 
  T1.ID,         
  T1.FECHA_SOLICITUD,         
  T22.ID_FLEXCUBE BHQ_CLIENTE,                 
  T22.RAZON_SOCIAL CLIENTE,                 
  T2.NOMBRE OPERACION,                 
  T221.DESCRIPCION PAIS,                 
  T2.USUARIO RESPONSABLE_OPERACION,                
  SUM(T4.MONTO_DESEMBOLSAR) MONTO,      
  T11.DESCRIPCION ESTADO,      
  T11.ID ID_ESTADO,      
  TO_CHAR(T2.ID_OPERACION) STR_ID_OPERACION,         
  T2.ID_OPERACION,                  
  T222.DESCRIPCION SECTOR_INSTITUCIONAL,
  T2.ESTADO ID_TCA_ESTADO_OPERACION 
  FROM SOLICITUD_DESEMBOLSO T1                   
    LEFT JOIN TCA_ESTADO_OPERACION T11 ON T11.ID = T1.ID_TCA_ESTADO                   
    INNER JOIN TCA_TIPO_ESTADO T111 ON T111.ID = T11.ID_TCA_TIPO_ESTADO                   
      AND T111.ID IN (2,3)                   
  INNER JOIN OPERACION T2 ON T2.ID_OPERACION = T1.ID_OPERACION      
  INNER JOIN CLIENTES T22 ON T22.ID_CLIENTE = T2.ID_CLIENTE                   
    LEFT JOIN CAT_PAISES T221 ON T221.ID = T22.PAIS                   
    LEFT JOIN CAT_SECTOR_INSTITUCIONAL T222 ON T222.ID = T22.SECTOR       
  LEFT JOIN TRE_SOLICITUD_LINEA_CREDITO T3 ON T3.ID_SOLICITUD = T1.ID      
  LEFT JOIN CONTRATO_DESEMBOLSO T4 ON T4.ID = T3.ID_CONTRATO_DESEMBOLSO       
        
  GROUP BY      
  T1.ID, T1.FECHA_SOLICITUD, T22.ID_FLEXCUBE, T22.RAZON_SOCIAL, T2.NOMBRE,       
  T221.DESCRIPCION, T2.USUARIO, T11.DESCRIPCION, T11.ID, T2.ID_OPERACION,       
  T2.ESTADO, T222.DESCRIPCION  
  ) QRSLTSQ1]]></SQLQuery>
