<?xml version="1.0"?>
<xsd:schema xmlns:tns="http://services.oracle.com/bpel/services/IdentityService"
            xmlns="http://xmlns.oracle.com/bpel/services/IdentityService"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:isc="http://www.oracle.com/pcbpel/identityservice/isconfig"
            xmlns:ns1="http://xmlns.oracle.com/bpel/services/IdentityService"
            targetNamespace="http://xmlns.oracle.com/bpel/services/IdentityService"
            elementFormDefault="qualified" attributeFormDefault="unqualified">
 <xsd:import namespace="http://www.oracle.com/pcbpel/identityservice/isconfig"
             schemaLocation="is_config.xsd"/>
 <!-- //////////////////// BPEL Process Manager Schemas//////////////////// -->
 <!-- **********************************
                 Type definitions
         *********************************** -->
 <xsd:simpleType name="LogicalOperatorEnumType">
  <xsd:restriction base="xsd:string">
   <xsd:enumeration value="AND"/>
   <xsd:enumeration value="OR"/>
  </xsd:restriction>
 </xsd:simpleType>
 <!--                                                  -->
 <xsd:simpleType name="UnaryOperatorEnumType">
  <xsd:restriction base="xsd:string">
   <xsd:enumeration value="NOT"/>
  </xsd:restriction>
 </xsd:simpleType>
 <!--                                                  -->
 <xsd:simpleType name="OperatorEnumType">
  <xsd:restriction base="xsd:string">
   <xsd:enumeration value="EQUAL"/>
   <xsd:enumeration value="NOTEQ"/>
   <xsd:enumeration value="LESS"/>
   <xsd:enumeration value="MORE"/>
   <xsd:enumeration value="MOREEQ"/>
   <xsd:enumeration value="EQLESS"/>
  </xsd:restriction>
 </xsd:simpleType>
 <!--                                                  -->
 <xsd:element name="roleClassifier" default="ALL_ROLES">
  <xsd:simpleType>
   <xsd:restriction base="xsd:string">
    <xsd:enumeration value="ALL_ROLES"/>
    <xsd:enumeration value="APP_ROLES"/>
    <xsd:enumeration value="ENT_ROLES"/>
   </xsd:restriction>
  </xsd:simpleType>
 </xsd:element>
 <!--                                                  -->
 <xsd:complexType name="FilterType">
  <xsd:choice maxOccurs="unbounded">
   <xsd:element ref="filter"/>
   <xsd:element ref="predicate"/>
  </xsd:choice>
  <xsd:attribute name="unaryOperator" type="UnaryOperatorEnumType"
                 use="optional" fixed="NOT"/>
  <xsd:attribute name="logicalOperator" type="LogicalOperatorEnumType"
                 use="optional" default="AND"/>
 </xsd:complexType>
 <!--                                                  -->
 <xsd:complexType name="PredicateType">
  <xsd:sequence>
   <xsd:element name="name" type="xsd:string"/>
   <xsd:element name="operator" type="OperatorEnumType"/>
   <xsd:element name="value" type="xsd:string"/>
  </xsd:sequence>
 </xsd:complexType>
 <!--         Deprecated                               -->
 <xsd:complexType name="ActionType">
  <xsd:sequence>
   <xsd:element name="name" type="xsd:string" minOccurs="0"/>
  </xsd:sequence>
 </xsd:complexType>
 <!--         Deprecated                               -->
 <xsd:complexType name="ActionListType">
  <xsd:sequence>
   <xsd:element ref="action" minOccurs="0" maxOccurs="unbounded"/>
  </xsd:sequence>
 </xsd:complexType>
 <xsd:complexType name="PermissionType">
  <xsd:sequence>
   <xsd:element name="className"/>
   <xsd:element name="targetName" type="xsd:string"/>
   <xsd:element name="actions" type="xsd:string" minOccurs="0"/>
  </xsd:sequence>
 </xsd:complexType>
 <!--                                                  -->
 <xsd:simpleType name="ValueType">
  <xsd:restriction base="xsd:string">
   <xsd:enumeration value="STRING"/>
   <xsd:enumeration value="BYTES"/>
  </xsd:restriction>
 </xsd:simpleType>
 <!--                                                  -->
 <xsd:complexType name="AttributeType">
  <xsd:sequence>
   <xsd:element name="name" type="xsd:string"/>
   <xsd:element name="value" type="xsd:string" maxOccurs="unbounded"/>
  </xsd:sequence>
  <xsd:attribute name="type" type="ValueType" use="optional" default="STRING"/>
 </xsd:complexType>
 <!--                                                  -->
 <xsd:complexType name="LocaleSpecificUserNamesResponseType">
