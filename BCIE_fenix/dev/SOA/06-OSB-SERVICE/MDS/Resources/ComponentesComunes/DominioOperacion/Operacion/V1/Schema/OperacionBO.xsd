    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ListaAccionesType">
    <xsd:sequence>
      <xsd:element name="Accion" maxOccurs="unbounded" minOccurs="0" type="operBO:AccionType"/>
