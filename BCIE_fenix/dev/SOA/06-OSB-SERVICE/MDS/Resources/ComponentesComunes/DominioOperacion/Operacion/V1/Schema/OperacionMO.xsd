  <!-- Create -->
  <!-- Carga de Operacions-->
  <xsd:complexType name="CrearOperacionRequestType">
    <xsd:sequence>
      <xsd:element name="Operacion" type="operBO:Operacion" minOccurs="1" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CrearOperacionResponseType">
    <xsd:sequence>
      <xsd:element name="Operacion" type="operBO:Operacion" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="Resultado" type="resBO:Resultado" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CrearOperacionAsociadaRequestType">
    <xsd:sequence>
      <xsd:element name="idOperacionPadre" type="operBO:idOperacion" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="idOperacionAsociada" type="operBO:idOperacion" minOccurs="1" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CrearOperacionAsociadaResponseType">
    <xsd:sequence>
      <xsd:element name="idOperacionPadre" type="operBO:idOperacion" minOccurs="1" maxOccurs="1"/>
