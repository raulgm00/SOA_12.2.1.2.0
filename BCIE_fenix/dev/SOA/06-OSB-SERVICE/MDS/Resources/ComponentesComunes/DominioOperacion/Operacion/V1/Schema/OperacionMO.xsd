        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="idProceso" type="xsd:long"/>
            <xsd:element name="nombreProceso" type="xsd:string"/>
            <xsd:element name="estatus" type="xsd:string"/>
            <xsd:element name="mensajeEstatus" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Resultado" type="resBO:Resultado" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- Fin Update -->
  <!-- Delete -->
  <xsd:complexType name="EliminarOperacionRequestType">
    <xsd:sequence>
      <xsd:element name="Operacion" type="operBO:idOperacion" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="borradoLogico" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
