      <xsd:element name="Resultado" type="resBO:Resultado" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- Crear cuestionario elegibilidad -->
  <xsd:complexType name="CrearCuestionarioRequestType">
