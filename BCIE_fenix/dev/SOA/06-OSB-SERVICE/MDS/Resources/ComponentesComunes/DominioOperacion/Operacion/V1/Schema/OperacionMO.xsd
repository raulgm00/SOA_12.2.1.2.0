      <xsd:element name="Resultado" type="resBO:Resultado"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- Fin Create -->
  <!-- Read -->
  <!-- Consulta de Operacions -->
  <xsd:complexType name="ConsultarOperacionByIdOperacionRequestType">
    <xsd:annotation>
      <xsd:documentation>Capacidad para consultar una Operacion por un Id de Operacion
          Usar el Id del Operacion para la consulta</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="idOperacion" type="operBO:idOperacion" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="nivelDetalle" type="AtributoBO:NivelType" default="OPERACION" minOccurs="0"/>
      <xsd:element name="infoDetalleCliente" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ConsultarOperacionResponseType">
    <xsd:sequence>
      <xsd:element name="Operacion" type="operBO:Operacion" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="Resultado" type="resBO:Resultado" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FiltrarOperacionesRequestType">
    <xsd:annotation>
      <xsd:documentation>Capacidad para consultar las operaciones de acuerdo a un filtro específico</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Operacion" type="operBO:Operacion" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="Cliente" type="clieBO:Cliente" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="offset" type="xsd:int" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="limit" type="xsd:int" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="FiltrarOperacionesResponseType">
    <xsd:sequence>
      <xsd:element name="ResultadoFiltro" maxOccurs="unbounded">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Operacion" type="operBO:Operacion" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="Cliente" type="clieBO:Cliente" minOccurs="0" maxOccurs="unbounded"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CantidadOperaciones" type="xsd:int" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="Resultado" type="resBO:Resultado" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ValidarExisteNombreOperacionRequestType">
    <xsd:sequence>
