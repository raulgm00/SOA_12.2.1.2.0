      <xsd:element name="Cuestionario" type="operBO:Cuestionario" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="Resultado" type="resBO:Resultado" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- Fin Read -->
  <!-- Update -->
  <xsd:complexType name="ActualizarOperacionRequestType">
    <xsd:sequence>
      <xsd:element name="Operacion" type="operBO:Operacion" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="Status" type="xsd:string"/>
      <xsd:element name="reasignar" type="xsd:boolean"/>
      <xsd:element name="actualizarTermino" type="xsd:boolean"/>
    </xsd:sequence>
