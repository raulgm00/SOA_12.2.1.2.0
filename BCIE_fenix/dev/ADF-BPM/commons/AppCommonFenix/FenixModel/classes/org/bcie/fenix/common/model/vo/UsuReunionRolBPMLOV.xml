  <SQLQuery><![CDATA[SELECT    
URA.ID URA_ID,    
URA.ID_SOLICITUD_APROBACION URA_ID_SOLICITUD_APROBACION,    
URA.ID_TCA_ROL_BPM URA_ID_TCA_ROL_BPM,    
URA.LOGIN_USUARIO URA_LOGIN_USUARIO,    
URA.FECHA_REGISTRO URA_FECHA_REGISTRO,    
URA.BAN_ESTATUS URA_BAN_ESTATUS,    
URA.EMITE_VOTO URA_EMITE_VOTO,    
( 
CASE WHEN (UPPER(URA.NOMBRE_USUARIO)) LIKE '%OTRO%' THEN 
  URA.NOMBRE_USUARIO||' - '||TRB.DESCRIPCION 
ELSE 
  URA.NOMBRE_USUARIO 
END 
) URA_NOMBRE_USUARIO, 
TRB.ID TRB_ID,    
TRB.DESCRIPCION TRB_DESCRIPCION,    
TRB.DESCRIPCION_CORTA TRB_DESCRIPCION_CORTA,    
TRB.BAN_ESTATUS TRB_BAN_ESTATUS,    
TRB.FECHA_REGISTRO TRB_FECHA_REGISTRO,    
TRB.COD_EXTERNO TRB_COD_EXTERNO    
FROM    
USUARIO_REUNION_APROBACION URA,    
TCA_ROL_BPM TRB    
WHERE 1=1    
AND URA.ID_TCA_ROL_BPM = TRB.ID]]></SQLQuery>
