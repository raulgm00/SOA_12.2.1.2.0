  <SQLQuery><![CDATA[SELECT                         
    ROWNUM AS ID,                             
    DOCUMENTO.ID_DOCUMENTO,                             
    ADJUNTO.ID_ADJUNTO,            
    Documentos.ID_OPERACION,         
    ADJUNTO.FILENAME,        
    TCA_DOCUMENTO.DESCRIPCION AS TIPO,         
    (SELECT DESCRIPCION FROM TCA_PROCESO_BPM WHERE ID = TCA_TAREA_BPM.ID_PROCESO_BPM) AS ETAPA,  
    DOCUMENTO.FECHA_REGISTRO AS FECHA_INICIO,   
    DOCUMENTO.FECHA_REGISTRO AS FECHA_FINAL  
FROM DOCUMENTO                             
INNER JOIN ADJUNTO ON DOCUMENTO.ID_DOCUMENTO = ADJUNTO.ID_DOCUMENTO                             
INNER JOIN TCA_DOCUMENTO ON DOCUMENTO.ID_TIPO_DOCUMENTO = TCA_DOCUMENTO.ID                             
INNER JOIN TCA_ACCION_DOCUMENTO On Documento.Id_Tca_Accion = TCA_ACCION_DOCUMENTO.ID           
INNER JOIN TCA_TAREA_BPM ON DOCUMENTO.ID_TAREA_BPM = TCA_TAREA_BPM.ID          
INNER JOIN documentos on DOCUMENTO.ID_DOCUMENTO=Documentos.ID_DOCUMENTO                            
WHERE TCA_ACCION_DOCUMENTO.ID <> 3      
AND Documentos.ID_OPERACION = :varIdOperacion     
AND DOCUMENTO.ID_DOCUMENTO NOT IN ( SELECT  ID_DOCUMENTO    
                                    FROM    TRE_EVIDENCIA_CONDICION   
                                    WHERE   ID_CONDICION  = :varIdCondicion AND AGRUPADOR = :pIdAgrupador)   
ORDER BY DOCUMENTO.ID_DOCUMENTO DESC]]></SQLQuery>
