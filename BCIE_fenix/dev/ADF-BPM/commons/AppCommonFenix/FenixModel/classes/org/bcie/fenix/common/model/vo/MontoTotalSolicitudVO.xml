OTAL_SOLICITUD"    Expression="MONTO_TOTAL_SOLICITUD"    SQLType="NUMERIC"/>  <AttrArray Name="KeyAttributes">    <Item Value="IdSolicitud"/>  </AttrArray>  <ClientInterface    Name="__clientInterface">    <Method      Name="precargarQuery"      MethodName="precargarQuery">      <Return        Name="_return_type_"        Type="oracle.jbo.Row"/>      <Parameter        Name="idSolicitud"        Type="java.lang.Long"/>    </Method>  </ClientInterface></ViewObject>