  <Variable
    Name="varIdPrepago"
    Kind="where"
    Type="java.lang.Long"/>
  <SQLQuery><![CDATA[SELECT PREPAGO.ID AS ID_PREPAGO,          
       PREPAGO.ID_TCA_TIPO_RESOLUCION,       
       PREPAGO.FECHA_PREPAGO,      
       TRE_PREPAGO_CONTRATO.ID AS ID_TRE_PREPAGO_CONTRATO,            
       DETALLE_PENALIDAD.ID AS ID_DETALLE_PENALIDAD,            
       TRE_PREPAGO_CONTRATO.CONTRATO_FLEXCUBE,            
       TCA_TIPO_MONEDA.DESCRIPCION_CORTA,            
       DETALLE_PENALIDAD.FECHA_INICIO,            
       DETALLE_PENALIDAD.FECHA_FIN,            
       DETALLE_PENALIDAD.PLAZO,            
       DETALLE_PENALIDAD.SPREAD,            
       DETALLE_PENALIDAD.FRACCION_LIBOR,            
       DETALLE_PENALIDAD.TASA_PREPAGO,            
       TRE_PREPAGO_CONTRATO.MONTO_PREPAGO,            
       DETALLE_PENALIDAD.MONTO_PENALIDAD,            
       DETALLE_PENALIDAD.INTERESES,            
       TRE_PREPAGO_CONTRATO.FECHA_PROXIMO_PAGO,  
       TRE_PREPAGO_CONTRATO.CAPITAL_NO_VENCIDO    
FROM PREPAGO             
INNER JOIN TRE_PREPAGO_CONTRATO             
  ON PREPAGO.ID = TRE_PREPAGO_CONTRATO.ID_PREPAGO             
INNER JOIN DETALLE_PENALIDAD             
  ON TRE_PREPAGO_CONTRATO.ID = DETALLE_PENALIDAD.ID_TRE_PREPAGO_CONTRATO             
INNER JOIN TCA_TIPO_MONEDA             
  ON TRE_PREPAGO_CONTRATO.ID_TCA_TIPO_MONEDA = TCA_TIPO_MONEDA.ID             
WHERE PREPAGO.ID = :varIdPrepago]]></SQLQuery>
