  <SQLQuery><![CDATA[SELECT ComentarioOperacionEO.ID,                           
       ComentarioOperacionEO.COMENTARIO,                           
       TO_CHAR(ComentarioOperacionEO.FECHA_REGISTRO, 'YYYY-MM-DD HH24:MI:SS') FECHA_REGISTRO,                           
       ComentarioOperacionEO.ID_INSTANCIA_TAREA,                           
       ComentarioOperacionEO.ID_OPERACION,                           
       ComentarioOperacionEO.ID_TCA_TAREA,                           
       ComentarioOperacionEO.LOGIN_USUARIO,                           
       ComentarioOperacionEO.NOMBRE_USUARIO,                         
       ComentarioOperacionEO.BAN_ESTATUS,                         
       (SELECT DESCRIPCION FROM TCA_TAREA_BPM WHERE ID = ComentarioOperacionEO.ID_TCA_TAREA) AS NombreTarea,                         
       (SELECT DESCRIPCION FROM TCA_PROCESO_BPM WHERE ID =(select ID_PROCESO_BPM  from TCA_TAREA_BPM where ID = ComentarioOperacionEO.ID_TCA_TAREA )) AS NombreProceso,     
       ComentarioOperacionEO.ID_TCA_TIPO_COMENTARIO, 
       (SELECT ID FROM TCA_PROCESO_BPM WHERE ID =(select ID_PROCESO_BPM  from TCA_TAREA_BPM where ID = ComentarioOperacionEO.ID_TCA_TAREA )) AS IdProceso 
FROM  COMENTARIO_OPERACION ComentarioOperacionEO                       
WHERE ComentarioOperacionEO.ID_OPERACION = :varIdOperacion AND                    
ComentarioOperacionEO.BAN_ESTATUS = 1 AND    
NVL(ComentarioOperacionEO.ID_TCA_TIPO_COMENTARIO,1) = 1       
ORDER BY ComentarioOperacionEO.ID DESC]]></SQLQuery>
