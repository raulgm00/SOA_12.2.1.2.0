                          disabled="#{(bindings.LineaGlobalIfi.inputValue eq true) and (pageFlowScope.pIdProducto eq 26)}"
                          shortDesc="#{bindings.NumeroLineaCredito.hints.tooltip}" id="it7"
                          label="#{PA11ImpPCTGHTBundle.NUMERO_DE_LINEA}:"
                          contentStyle="width: 245px;">
              <f:validator binding="#{bindings.NumeroLineaCredito.validator}"/>
              <af:validateLength maximum="16"
