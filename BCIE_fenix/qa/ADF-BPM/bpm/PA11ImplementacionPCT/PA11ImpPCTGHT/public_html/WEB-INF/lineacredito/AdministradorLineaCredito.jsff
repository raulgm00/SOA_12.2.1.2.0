                                showRequired="true"
                                shortDesc="#{bindings.CatProductoFlexcubeLOV.hints.tooltip}" id="soc3"
                                contentStyle="width: 250px;">
              <f:selectItems value="#{bindings.CatProductoFlexcubeLOV.items}" id="si3"/>
              <f:validator binding="#{bindings.CatProductoFlexcubeLOV.validator}"/>
            </af:selectOneChoice>
          </af:panelFormLayout>
        </af:panelHeader>
        <af:spacer width="10" height="10" id="s3"/>
