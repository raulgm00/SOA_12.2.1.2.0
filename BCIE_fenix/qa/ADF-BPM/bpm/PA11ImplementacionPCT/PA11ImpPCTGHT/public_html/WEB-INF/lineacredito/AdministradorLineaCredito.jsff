            <af:panelGroupLayout id="pgl7" layout="horizontal">
              <af:spacer width="20" height="10" id="s14"/>
              <af:outputText value="#{PA11ImpPCTGHTBundle.MONTO_TOTAL}" 
                             id="ot1" inlineStyle="font-weight:bold;">
                <f:convertNumber maxFractionDigits="3" minFractionDigits="2" groupingUsed="true" type="number" locale="en"/>
              </af:outputText>
            </af:panelGroupLayout>
          </f:facet>-->
        </af:column>
        
        <!-- Monto Disponible -->
        <!--<af:column headerText="#{bindings.DatosLineaCreditoVO.hints.MontoDisponible.label}" id="md_c2"
                   rowHeader="unstyled" width="300" align="center" inlineStyle="text-align:center;">
          <af:outputText value="#{row.bindings.MontoDisponible.inputValue}"
                         shortDesc="#{bindings.DatosLineaCreditoVO.hints.MontoDisponible.tooltip}" id="md_it4">
              <f:convertNumber maxFractionDigits="3" minFractionDigits="2" groupingUsed="true" type="number" locale="en"/>
          </af:outputText>
        </af:column>-->
      </af:table>
    </af:panelGroupLayout>
    <af:spacer width="10" height="10" id="s13"/>
    
    <?audit suppress oracle.jdeveloper.jsp.validate-children?>
