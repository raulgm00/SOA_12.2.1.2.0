            <af:panelGroupLayout id="pgl7" layout="horizontal" halign="center">
              <af:outputText value="#{PA11ImpPCTGHTBundle.MONTO_TOTAL}" 
                             id="ot1" inlineStyle="font-weight:bold;">
                <f:convertNumber maxFractionDigits="3" minFractionDigits="2" groupingUsed="true" type="number" locale="en"/>
              </af:outputText>
            </af:panelGroupLayout>
          </f:facet>
        </af:column>
        
        <!-- Monto de la linea -->
        <af:column headerText="#{bindings.DatosLineaCreditoVO.hints.MontoLinea.label}" id="c4" width="400"
                   align="center" inlineStyle="text-align:center;">
          <af:outputText value="#{row.bindings.MontoLinea.inputValue}"
                         shortDesc="#{bindings.DatosLineaCreditoVO.hints.MontoLinea.tooltip}" id="it6">
            <f:convertNumber maxFractionDigits="3" minFractionDigits="2" groupingUsed="true" type="number" locale="en_US"/>
          </af:outputText>
          <f:facet name="footer">
            <af:panelGroupLayout id="pgl8" layout="horizontal" halign="center">
                <af:outputText value="#{bindings.MontoTotal.inputValue}" shortDesc="#{bindings.MontoTotal.hints.tooltip}"
                     id="ot10">
                    <f:convertNumber maxFractionDigits="3" minFractionDigits="2" groupingUsed="true" type="number" locale="en_US"/>
                </af:outputText>
            </af:panelGroupLayout>
          </f:facet>
        </af:column>
        
        <!-- Fondo Contable -->        
        <af:column headerText="#{bindings.DatosLineaCreditoVO.hints.Fondo.label}" id="c3" width="400" align="center"
                   inlineStyle="text-align:center;">
          <af:outputText value="#{row.bindings.Fondo.inputValue}"
                         shortDesc="#{bindings.DatosLineaCreditoVO.hints.Fondo.tooltip}" id="it5"/>
