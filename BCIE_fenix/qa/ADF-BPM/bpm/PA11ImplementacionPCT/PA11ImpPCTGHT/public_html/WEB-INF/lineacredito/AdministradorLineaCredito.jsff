                              shortDesc="#{bindings.IdTipoMoneda2.hints.tooltip}" id="soc16" readOnly="true">
            <f:selectItems value="#{bindings.IdTipoMoneda2.items}" id="si18"/>
            <f:validator binding="#{bindings.IdTipoMoneda2.validator}"/>
