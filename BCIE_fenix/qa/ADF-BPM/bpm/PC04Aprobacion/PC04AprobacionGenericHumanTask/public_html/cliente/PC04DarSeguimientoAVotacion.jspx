                                                            <af:spacer width="10" height="10" id="s12"/>
                                                            <af:button text="#{pc04aprobaciongenerichumantaskBundle.LBL_EDITAR_VOTO_FUERA_DE_SISTEMA}"
                                                                       id="btnEditarVoto"
                                                                       action="#{aprobacionActionsManagedBean.editarVotoFueraSistema}"
                                                                       visible="false" rendered="false"/>
                                                            -->
                                                        </af:panelGroupLayout>
                                                        <af:spacer width="10" height="10" id="s14"/>
                                                        <af:treeTable summary="*"
                                                                      value="#{bindings.VotoActualAprobLOV1.treeModel}"
                                                                      var="node"
                                                                      binding="#{pageFlowScope.aprobacionManagedBean.treeTabBind}"
                                                                      selectionListener="#{bindings.VotoActualAprobLOV1.treeModel.makeCurrent}"
                                                                      rowSelection="single" id="tt1"
                                                                      columnStretching="multiple"
                                                                      emptyText="#{bindings.VotoActualAprobLOV1.viewable ? pc04AprobacionGenericHumanTaskBundle['tabla_datos_no_encontrados'] : pc04AprobacionGenericHumanTaskBundle['tabla_acceso_denegado']}"
                                                                      inlineStyle="margin-left:-12px;">
                                                            <f:facet name="nodeStamp">
                                                                <af:column id="c1" rowHeader="true" width="20%"
                                                                           align="left"
                                                                           headerText="#{pc04AprobacionGenericHumanTaskBundle.LBL_MIEMBRO_COMITE}">
                                                                    <af:outputText value="#{node.TrbDescripcion} #{node.VaEsFueraDeSistema eq 1 ? pc04AprobacionGenericHumanTaskBundle.pc04aprobacion_dar_seguimiento_votacion_fuera_sistema_label : ''}"
                                                                                   id="otD"/>
                                                                </af:column>
                                                            </f:facet>
                                                            <af:column id="c2" width="20%"
                                                                       headerText="#{pc04AprobacionGenericHumanTaskBundle.LBL_FECHA1}">
                                                                <af:inputDate label="" id="id6"
                                                                              value="#{node.VaFechaHora}"
                                                                              readOnly="true"
                                                                              disabled="#{pageFlowScope.aprobacionManagedBean.esEstadoCompletado}">
                                                                    <af:convertDateTime pattern="dd/MMM/yyyy"/>
                                                                </af:inputDate>
                                                            </af:column>
                                                            <af:column id="c3" width="20%"
                                                                       headerText="#{pc04AprobacionGenericHumanTaskBundle.LBL_DECISION}">
                                                                <af:outputText value="#{node.TtdDescripcion}"
                                                                               id="otMC"/>
                                                            </af:column>
                                                            <af:column id="c4" width="40%"
                                                                       headerText="#{pc04AprobacionGenericHumanTaskBundle.LBL_COMENTARIO1}">
                                                                <af:outputText value="#{node.Comentario}" id="otC"/>
                                                            </af:column>
                                                            <f:facet name="pathStamp">
                                                                <af:outputText value="#{node.HistoricoVotoAprobacionLOV}"
                                                                               id="ot4"/>
                                                            </f:facet>
                                                        </af:treeTable>
                                                    </af:panelGroupLayout>
                                                </af:panelHeader>
                                            </af:panelGroupLayout>
                                        </af:panelHeader>
                                        <af:spacer width="10" height="10" id="s19"/>
                                        <af:separator id="s15"/>
                                        <af:spacer width="10" height="10" id="s18"/>
                                        <af:panelFormLayout id="pfl5" labelAlignment="top" fieldWidth="100%"
                                                            maxColumns="1" rows="2">
                                            <af:inputText label="#{pc04AprobacionGenericHumanTaskBundle.LBL_COMENTARIO}"
                                                          id="it3" rows="3" styleClass="AFStretchWidth"
                                                          inlineStyle="grid-column-sizing:100%; grid-row-sizing:100%;"
                                                          value="#{pageFlowScope.aprobacionManagedBean.comentario}"
                                                          partialTriggers="b2"
                                                          placeholder="#{pc04AprobacionGenericHumanTaskBundle.LBL_PLACEHOLDER_COMENTARIO}"
                                                          autoSubmit="true"
                                                          disabled="#{pageFlowScope.aprobacionManagedBean.esEstadoCompletado}"/>
                                            <f:facet name="footer">
                                                <af:button text="#{pc04AprobacionGenericHumanTaskBundle.LBL_AGREGAR_COMENTARIO}"
                                                           id="b2"
                                                           action="#{aprobacionActionsManagedBean.agregarComentario}"
                                                           binding="#{aprobacionActionsManagedBean.agregarComentarioRButtonUIC}"
                                                           disabled="#{ (empty pageFlowScope.aprobacionManagedBean.comentario)
                                                                            or pageFlowScope.aprobacionManagedBean.esEstadoCompletado}"
                                                           partialTriggers="it3"/>
                                            </f:facet>
                                        </af:panelFormLayout>
                                        <af:spacer width="10" height="10" id="s20"/>
                                        <af:listView value="#{bindings.ComentarioReunionVO.collectionModel}" var="item"
                                                     emptyText="#{bindings.ComentarioReunionVO.viewable ? pc04AprobacionGenericHumanTaskBundle['tabla_datos_no_encontrados'] : pc04AprobacionGenericHumanTaskBundle['tabla_acceso_denegado']}"
                                                     fetchSize="#{bindings.ComentarioReunionVO.rangeSize}" id="lv1"
                                                     partialTriggers="::b2"
                                                     binding="#{aprobacionActionsManagedBean.listViewComentariosReunion}">
                                            <af:listItem id="li1">
                                                <af:panelGridLayout id="pg20">
                                                    <af:gridRow marginTop="5px" height="auto" id="gr3">
                                                        <af:gridCell marginStart="5px" width="34%" id="gc7">
                                                            <af:panelLabelAndMessage label="#{pc04AprobacionGenericHumanTaskBundle.LBL_DATOS_COMENTARIO_USUARIO}"
                                                                                     id="plam3">
                                                                <af:outputText value="#{item.NombreUsuario}" id="ot3"/>
                                                            </af:panelLabelAndMessage>
                                                        </af:gridCell>
                                                        <af:gridCell marginStart="5px" width="33%" id="gc8">
                                                            <af:inputDate label="#{pc04AprobacionGenericHumanTaskBundle.LBL_DATOS_COMENTARIO_FECHA}"
                                                                          id="id3" value="#{item.FechaHora}"
                                                                          readOnly="true"
                                                                          disabled="#{pageFlowScope.aprobacionManagedBean.esEstadoCompletado}">
                                                                <af:convertDateTime pattern="dd/MM/yyyy"/>
                                                            </af:inputDate>
                                                        </af:gridCell>
                                                        <af:gridCell marginStart="5px" width="33%" marginEnd="5px"
                                                                     id="gc9">
                                                            <af:inputDate label="#{pc04AprobacionGenericHumanTaskBundle.LBL_DATOS_COMENTARIO_HORA}"
                                                                          id="id4" value="#{item.FechaHora}"
                                                                          readOnly="true"
                                                                          disabled="#{pageFlowScope.aprobacionManagedBean.esEstadoCompletado}">
                                                                <af:convertDateTime pattern="HH:mm"/>
                                                            </af:inputDate>
                                                        </af:gridCell>
                                                    </af:gridRow>
                                                    <af:gridRow marginTop="5px" height="auto" marginBottom="5px"
                                                                id="gr4">
                                                        <af:gridCell marginStart="5px" width="33%" marginEnd="5px"
                                                                     id="gc12">
                                                            <af:activeOutputText value="#{item.Comentario}" id="aot1"/>
                                                        </af:gridCell>
                                                    </af:gridRow>
                                                </af:panelGridLayout>
                                            </af:listItem>
                                        </af:listView>
                                    </af:panelGroupLayout>
                                    <af:spacer width="10" height="10" id="s10"/>
                                    <!-- LISTADO DE "COMITE DE CRDITO", "CON NOTIFICACION" Y "EQUIPO DE TRABAJO" -->
                                    <af:panelGroupLayout id="pgl1">
                                        <af:region value="#{bindings.miembrosAprobTaskFlow1.regionModel}" id="r7"/>
                                    </af:panelGroupLayout>
                                </af:showDetailItem>
                                <af:showDetailItem id="pane6" text="#{pc04AprobacionGenericHumanTaskBundle.LBL_COMENTARIOS}" immediate="true"
                                                   disclosureListener="#{pageFlowScope.aprobacionManagedBean.panelComentariosDisclosureListener}"
                                                   inlineStyle="margin-left: 10px;">
                                    <af:region value="#{bindings.comentariosClienteBTF1.regionModel}" id="r6"/>
                                </af:showDetailItem>
                            </af:panelAccordion>
                        </af:panelGroupLayout>
                    </f:facet>
                    <f:facet name="comment"/>
                    <f:facet name="attachment"/>
                    <f:facet name="history"/>
                </af:pageTemplate>
                <af:popup childCreation="deferred" id="popupVotoFueraDeSistema" contentDelivery="lazyUncached"
                          binding="#{aprobacionActionsManagedBean.popupAgregarModificarVoto}"
                          popupCanceledListener="#{aprobacionActionsManagedBean.cancelPopupAgregarModificarVoto}"
                          partialTriggers="pageTemplateID:btnAgregarVoto">
                    <af:dialog id="d3" type="okCancel" contentHeight="180" contentWidth="600" resize="on"
                               closeIconVisible="false" stretchChildren="first"
                               title="#{pageFlowScope.aprobacionManagedBean.tituloPopup}"
                               partialTriggers="pageTemplateID:btnAgregarVoto"
                               dialogListener="#{aprobacionActionsManagedBean.dialogListenerAgregarModificarVotoCliente}"
                               cancelTextAndAccessKey="#{pc04AprobacionGenericHumanTaskBundle.LBL_BTN_CANCELAR}"
                               affirmativeTextAndAccessKey="#{pc04AprobacionGenericHumanTaskBundle.LBL_BTN_ACEPTAR}">
                        <f:facet name="buttonBar"/>
                        <af:panelGroupLayout id="pgl8" layout="vertical">
                            <af:panelFormLayout id="pfl9" labelAlignment="top" maxColumns="1" rows="3">
                                <af:selectOneChoice value="#{bindings.idRolBPMVarList.inputValue}"
                                                    label="#{pc04AprobacionGenericHumanTaskBundle.pc04aprobacion_consolidar_decision_tabla_votos_miembros_comite_label}"
                                                    required="true"
                                                    shortDesc="#{bindings.idRolBPMVarList.hints.tooltip}" id="socO"
                                                    autoSubmit="true" immediate="true"
                                                    valueChangeListener="#{aprobacionActionsManagedBean.miembroVotacionChangeListener}"
                                                    requiredMessageDetail="#{pc04AprobacionGenericHumanTaskBundle.pc04aprobacion_dar_seguimiento_votacion_agregar_editar_msg_comite_requerido}">
                                    <f:selectItems value="#{bindings.idRolBPMVarList.items}" id="sic"/>
                                    <f:validator binding="#{bindings.idRolBPMVarList.validator}"/>
                                </af:selectOneChoice>
                                <af:selectOneChoice value="#{bindings.IdTcaTipoDecision.inputValue}"
                                                    label="#{bindings.IdTcaTipoDecision.label}" required="true"
                                                    shortDesc="#{bindings.IdTcaTipoDecision.hints.tooltip}" id="soc1"
                                                    autoSubmit="true" immediate="true"
                                                    valueChangeListener="#{aprobacionActionsManagedBean.decisionVotacionChangeListener}"
                                                    requiredMessageDetail="#{pc04AprobacionGenericHumanTaskBundle.pc04aprobacion_dar_seguimiento_votacion_agregar_editar_msg_decision_requerido}">
                                    <f:selectItems value="#{bindings.IdTcaTipoDecision.items}" id="si1"/>
                                    <f:validator binding="#{bindings.IdTcaTipoDecision.validator}"/>
                                </af:selectOneChoice>
                                <af:inputText value="#{bindings.ComentarioFueraSistema.inputValue}"
                                              label="#{pc04AprobacionGenericHumanTaskBundle.LBL_COMENTARIO1}"
                                              maximumLength="#{bindings.ComentarioFueraSistema.hints.precision}"
                                              shortDesc="#{bindings.ComentarioFueraSistema.hints.tooltip}" id="it5"
                                              rows="3" columns="80"
                                              valueChangeListener="#{aprobacionActionsManagedBean.comentarioVotacionChangeListener}"/>
                            </af:panelFormLayout>
                        </af:panelGroupLayout>
                    </af:dialog>
                </af:popup>
            </af:form>
        </af:document>
    </f:view>
</jsp:root>

