                                                            <af:button text="#{pc04AprobacionGenericHumanTaskBundle.LBL_AGREGAR_VOTO_FUERA_DE_SISTEMA}"
                                                                       id="btnAgregarVoto" immediate="true"
                                                                       actionListener="#{aprobacionActionsManagedBean.votoFuera}"
                                                                       disabled="#{pageFlowScope.aprobacionManagedBean.esEstadoCompletado}"/>
