    <!-- CLIENTE START -->
    <xsl:template match="CLIENTE">
			<xsl:choose>
	            <xsl:when test="/CLIENTES/CONFIGURACION/TIPO_DOCUMENTO = 'AVISO'">
					<xsl:variable name="cliente">
						<CLIENTES>
						<CLIENTE>
							<xsl:copy-of select="FECHA_EMISION"/>
							<xsl:copy-of select="CODIGO_CLIENTE"/>
							<xsl:copy-of select="RAZON_SOCIAL"/>
							<xsl:copy-of select="COUNTRY"/>
							<xsl:copy-of select="CUENTAS"/>
							<xsl:copy-of select="LINEA[VENCIMIENTOS/VENCIMIENTO[VENCIDO='N']]"/>										
						</CLIENTE>
							<xsl:copy-of select="/CLIENTES/CONFIGURACION"/>
						</CLIENTES>
					</xsl:variable>
	                <xsl:choose>
						<xsl:when test="/CLIENTES/CONFIGURACION/NIVEL_DETALLE = 'CONSOLIDADO'">
							<xsl:apply-templates select="$cliente/CLIENTES/CLIENTE" mode="consolidated">
							</xsl:apply-templates>
						</xsl:when>
						<xsl:otherwise>
							<xsl:apply-templates select="$cliente/CLIENTES/CLIENTE" mode="detail">
							</xsl:apply-templates>
						</xsl:otherwise>
					</xsl:choose>
	            </xsl:when>
	            <xsl:otherwise>
	                <xsl:variable name="cliente">
						<CLIENTES>
						<CLIENTE>
							<xsl:copy-of select="FECHA_EMISION"/>
							<xsl:copy-of select="CODIGO_CLIENTE"/>
							<xsl:copy-of select="RAZON_SOCIAL"/>
							<xsl:copy-of select="COUNTRY"/>
							<xsl:copy-of select="CUENTAS"/>
							<xsl:copy-of select="LINEA[VENCIMIENTOS/VENCIMIENTO[VENCIDO='S']]"/>										
						</CLIENTE>
							<xsl:copy-of select="/CLIENTES/CONFIGURACION"/>
						</CLIENTES>
					</xsl:variable>
					<xsl:choose>
						<xsl:when test="/CLIENTES/CONFIGURACION/NIVEL_DETALLE = 'CONSOLIDADO'">
							<xsl:apply-templates select="$cliente/CLIENTES/CLIENTE" mode="consolidated">
							</xsl:apply-templates>
						</xsl:when>
						<xsl:otherwise>
							<xsl:apply-templates select="$cliente/CLIENTES/CLIENTE" mode="detail">
							</xsl:apply-templates>
						</xsl:otherwise>
					</xsl:choose>
	            </xsl:otherwise>
	        </xsl:choose>
    </xsl:template>
    <!-- CLIENTE END -->

	<!-- CLIENTE.CONSOLIDADO START-->
	<xsl:template match="CLIENTE" mode="consolidated">
		<xsl:for-each-group select="LINEA"
				group-by="concat(OPERACION, '|', VENCIMIENTOS/VENCIMIENTO[1]/MONEDA, '|', DESCRIPCION_LINEA)">
                <?audit suppress oracle.ide.xml.validation-error?>
				<xsl:sort select="OPERACION"/>
                <!--ENCABEZADO / CARATULA-->
					<xsl:for-each-group select="current-group()"
                    group-by="VENCIMIENTOS/VENCIMIENTO[1]/FECHA_VENCIMIENTO">
					<xsl:sort select="VENCIMIENTOS/VENCIMIENTO[1]/FECHA_VENCIMIENTO_ORDEN"/>
						<!-- ENCABEZADP/INFORMACION GENERAL -->
						<xsl:choose>
							<xsl:when test="/CLIENTES/CONFIGURACION/TIPO_GENERACION = 'DETALLE'">
								<xsl:choose>
									<xsl:when test="position() = 1">
										<xsl:apply-templates select="." mode="header">
											<xsl:with-param name="soloFecha">N</xsl:with-param>
											<xsl:with-param name="detalle">N</xsl:with-param>
											<xsl:with-param name="ocultarFecha">S</xsl:with-param>
										</xsl:apply-templates>
									</xsl:when>
									<xsl:otherwise>
									</xsl:otherwise>
								</xsl:choose>
							</xsl:when>
							<xsl:otherwise>
								<xsl:choose>
						        	<xsl:when test="position() = 1">
						            	<xsl:apply-templates select="." mode="header">
											<xsl:with-param name="soloFecha">N</xsl:with-param>
											<xsl:with-param name="detalle">N</xsl:with-param>
											<xsl:with-param name="ocultarFecha">N</xsl:with-param>
										</xsl:apply-templates>
										<w:p w:rsidR="00601A5A" w:rsidRDefault="009D665E">
											<w:pPr>
												<w:keepNext/>
												<w:keepLines/>
											</w:pPr>
								            <w:r>
								                <w:t xml:space="preserve"> </w:t>
								            </w:r>
								            <w:bookmarkStart w:id="0" w:name="_GoBack"/>
								            <w:bookmarkEnd w:id="0"/>
								        </w:p>
						             </xsl:when>
						             <xsl:otherwise>
						             	<xsl:apply-templates select="." mode="header">
											<xsl:with-param name="soloFecha">S</xsl:with-param>
											<xsl:with-param name="detalle">N</xsl:with-param>
											<xsl:with-param name="ocultarFecha">N</xsl:with-param>
										</xsl:apply-templates>
						        	</xsl:otherwise>
						    	</xsl:choose>
							</xsl:otherwise>
						</xsl:choose>
						<!-- SI EL TIPO DE GENERACION ES CARATULA O AMBOS SE MUESTRAN LAS TABLAS-->
						<xsl:if test="/CLIENTES/CONFIGURACION/TIPO_GENERACION != 'DETALLE'">
							<!-- SI EL TIPO DE DOCUMENTO DISTINTO DE MORA -->
							
							<xsl:variable name="paraVencidos">
								<xsl:choose>
									<xsl:when test="/CLIENTES/CONFIGURACION/TIPO_DOCUMENTO = 'MORA'">S</xsl:when>
									<xsl:otherwise>N</xsl:otherwise>
								</xsl:choose>
							</xsl:variable>
							
							<xsl:apply-templates select="current-group()/VENCIMIENTOS[count(VENCIMIENTO[VENCIDO=$paraVencidos])>0]" mode="revision"/>
							
							<w:tbl>
								<w:tblPr>
									<w:tblStyle w:val="BCIE-Blue"/>
									<w:tblW w:w="10800" w:type="dxa"/>
									<w:tblLook w:val="04A0" w:firstRow="1" w:lastRow="1" w:firstColumn="0" w:lastColumn="0" w:noHBand="1"
											   w:noVBand="1"/>
								</w:tblPr>
								<w:tblGrid>
									<w:gridCol w:w="1480"/>
									<w:gridCol w:w="1480"/>
									<w:gridCol w:w="1568"/>
									<w:gridCol w:w="1568"/>
									<w:gridCol w:w="1568"/>
									<w:gridCol w:w="1568"/>
									<w:gridCol w:w="1568"/>
								</w:tblGrid>
								
								<xsl:for-each select="current-group()">
									<xsl:sort select="NUMERO_LINEA_CREDITO"/>
									<xsl:choose>
										<xsl:when test="position() = 1">
											<xsl:apply-templates select="VENCIMIENTOS" mode="consolidated">
												<xsl:with-param name="paraVencidos"><xsl:value-of select="$paraVencidos"/></xsl:with-param>
											</xsl:apply-templates>
										</xsl:when>
										<xsl:otherwise></xsl:otherwise>
									</xsl:choose>
									
									<xsl:apply-templates select="VENCIMIENTOS" mode="consolidatedDetail">
										<xsl:with-param name="paraVencidos"><xsl:value-of select="$paraVencidos"/></xsl:with-param>
									</xsl:apply-templates>		
									
								</xsl:for-each>
								
								<xsl:variable name="linea">
									<LINEA>
										<VENCIMIENTOS>
											<xsl:copy-of select="current-group()/VENCIMIENTOS/VENCIMIENTO"/>
										</VENCIMIENTOS>
									</LINEA>
								</xsl:variable>
							
								<xsl:apply-templates select="$linea/LINEA" mode="foot_vencimiento">
									<xsl:with-param name="agrupar">2</xsl:with-param>
									<xsl:with-param name="titulo">Total a pagar <xsl:value-of select="$linea/LINEA/VENCIMIENTOS/VENCIMIENTO[1]/FECHA_VENCIMIENTO"/></xsl:with-param>
								</xsl:apply-templates>			
							</w:tbl>
								
								
						</xsl:if>
  					</xsl:for-each-group>

  					<!--CUENTAS-->
					<xsl:for-each-group select="current-group()/VENCIMIENTOS/VENCIMIENTO"
                    group-by="concat(FONDO, '|', ../../NUMERO_LINEA_CREDITO)">
						<xsl:sort select="../../NUMERO_LINEA_CREDITO"/>
						<w:p w:rsidR="00601A5A" w:rsidRDefault="009D665E">
				            <w:r>
				                <w:t xml:space="preserve"> </w:t>
				            </w:r>
				            <w:bookmarkStart w:id="0" w:name="_GoBack"/>
				            <w:bookmarkEnd w:id="0"/>
				        </w:p>
						<xsl:if test="position() = 1">
							<w:p w:rsidR="00601A5A" w:rsidRDefault="009D665E">
								<w:r>
									<w:t xml:space="preserve"> </w:t>
								</w:r>
								<w:bookmarkStart w:id="0" w:name="_GoBack"/>
								<w:bookmarkEnd w:id="0"/>
							</w:p>
							<w:p w:rsidRDefault="00BAD77">
								<w:pPr>
									<w:keepNext/>
									<w:keepLines/>
								</w:pPr>
								<w:r>
									<w:t>El pago deberá realizarse de acuerdo con las siguientes instrucciones de pago:</w:t>
								</w:r>
							</w:p>
							<w:p w:rsidR="00601A5A" w:rsidRDefault="009D665E">
								<w:pPr>
									<w:keepNext/>
									<w:keepLines/>
								</w:pPr>
								<w:r>
									<w:t xml:space="preserve"> </w:t>
								</w:r>
								<w:bookmarkStart w:id="0" w:name="_GoBack"/>
								<w:bookmarkEnd w:id="0"/>
							</w:p>
						</xsl:if>
						<xsl:apply-templates select="/CLIENTES/CLIENTE/CUENTAS">
				            <xsl:with-param name="paraCodigo_Fondo"><xsl:value-of select="FONDO"/></xsl:with-param>
				            <xsl:with-param name="paraCodigo_Moneda"><xsl:value-of select="MONEDA"/></xsl:with-param>
				            <xsl:with-param name="paraMonto"><xsl:value-of select="sum(current-group()/TOTAL)"/></xsl:with-param>
				            <xsl:with-param name="paraNumero_Linea_Credito"><xsl:value-of select="../../NUMERO_LINEA_CREDITO"/></xsl:with-param>
				        </xsl:apply-templates>			
  					</xsl:for-each-group>

					<!--DETALLE-->
					<!--por instrucciones de COFI se hace un page break previo a el detalle
					 esto solo si es nivel detallado u ambos-->
					 <xsl:if test="/CLIENTES/CONFIGURACION/TIPO_GENERACION != 'CARATULA'">
						<!-- En caso de que si se muestre el detalle, se hace el salto de pagina previo a colocarse-->
						<w:p>
			                <w:r>
			                    <w:br w:type="page"/>
			                </w:r>
			            </w:p>
						<w:tbl>
							<w:tblPr>
								<w:tblStyle w:val="BCIE-Blue"/>
								<w:tblW w:w="10800" w:type="dxa"/>
								<w:tblLook w:val="04A0" w:firstRow="1" w:lastRow="1" w:firstColumn="0" w:lastColumn="0" w:noHBand="1"
										   w:noVBand="1"/>
							</w:tblPr>
							<w:tblGrid>
								<w:gridCol w:w="10800"/>
							</w:tblGrid>
							<w:tr>
								<w:trPr>
									<w:cnfStyle w:firstRow="1"/>
									<w:trHeight w:val="247"/>
									<w:tblHeader/>
								</w:trPr>
								<w:tc>
									<w:tcPr>
										<w:tcW w:w="10800" w:type="dxa"/>
										<w:shd w:val="clear" w:color="auto" w:fill="4F81BD"/>
									</w:tcPr>
									<w:p>
										<w:pPr>
											<w:jc w:val="center"/>
											<w:rPr>
												<w:b/>
											</w:rPr>
										</w:pPr>
										<w:r>
											<w:rPr>
												<w:color w:val="FFFFFF"/>
												<w:b/>
											</w:rPr>
											<w:t>Detalle de Cálculos</w:t>
										</w:r>
									</w:p>
								</w:tc>
							</w:tr>
						</w:tbl>
						<xsl:for-each-group select="current-group()"
                    	group-by="concat(VENCIMIENTOS/VENCIMIENTO[1]/FECHA_VENCIMIENTO, '|', NUMERO_LINEA_CREDITO)">
						<xsl:sort select="VENCIMIENTOS/VENCIMIENTO[1]/FECHA_VENCIMIENTO_ORDEN"/>
						<xsl:sort select="NUMERO_LINEA_CREDITO"/>
							<!-- SE COLOCA LA FECHA DE CADA UNO-->
							<xsl:apply-templates select="." mode="header">
									<xsl:with-param name="soloFecha">S</xsl:with-param>
									<xsl:with-param name="detalle">S</xsl:with-param>
									<xsl:with-param name="ocultarFecha">S</xsl:with-param>
							</xsl:apply-templates>
							
							<xsl:for-each-group select="VENCIMIENTOS/VENCIMIENTO" group-by="FONDO">
								<w:p w:rsidR="00601A5A" w:rsidRDefault="009D665E">
									<w:r>
										<w:t xml:space="preserve"><xsl:value-of select="FONDO"/> - <xsl:value-of select="DESCRIPCION_FONDO"/></w:t>
									</w:r>
									<w:bookmarkStart w:id="0" w:name="_GoBack"/>
									<w:bookmarkEnd w:id="0"/>
								</w:p>
							</xsl:for-each-group> 

							<xsl:variable name="vencimientosInteres">
									<LINEA>
										<NUMERO_LINEA_CREDITO>
											<xsl:value-of select="NUMERO_LINEA_CREDITO"/>
										</NUMERO_LINEA_CREDITO>										
										<VENCIMIENTOS>
											<xsl:copy-of select="current-group()/VENCIMIENTOS[count(VENCIMIENTO/CALCULOS/CALCULO[COMPONENTE_AGRUPADO='Intereses'])>0]/VENCIMIENTO"/>
										</VENCIMIENTOS>
									</LINEA>
							</xsl:variable>
							<xsl:if test="count($vencimientosInteres/LINEA/VENCIMIENTOS/VENCIMIENTO) > 0">
			                	<xsl:apply-templates select="$vencimientosInteres/LINEA/VENCIMIENTOS"
			                    	                 mode="calculoConsolidated">
			                    	<xsl:with-param name="paraComponente">Intereses</xsl:with-param>
			                    	<xsl:with-param name="tituloComponente">Intereses</xsl:with-param>
			                	</xsl:apply-templates>
			                </xsl:if>

							<xsl:variable name="vencimientosComision">
									<LINEA>
										<xsl:copy-of select="NUMERO_LINEA_CREDITO"/>										
										<VENCIMIENTOS>
											<xsl:copy-of select="current-group()/VENCIMIENTOS[count(VENCIMIENTO/CALCULOS/CALCULO[COMPONENTE_AGRUPADO='Comision de Compr.'])>0]/VENCIMIENTO"/>
										</VENCIMIENTOS>
									</LINEA>
							</xsl:variable>
							<xsl:if test="count($vencimientosComision/LINEA/VENCIMIENTOS/VENCIMIENTO) > 0">
								<xsl:apply-templates select="$vencimientosComision/LINEA/VENCIMIENTOS"
			                    	                 mode="calculoConsolidated">
			                    	<xsl:with-param name="paraComponente">Comision de Compr.</xsl:with-param>
			                    	<xsl:with-param name="tituloComponente">Comisión de Compromiso</xsl:with-param>
			                	</xsl:apply-templates>
							</xsl:if>
						</xsl:for-each-group>
					 </xsl:if>

					 <!-- SALTO ENTRE CADA GRUPO-->
					<xsl:if test="position() != last()">
			            <w:p>
			                <w:r>
			                    <w:br w:type="page"/>
			                </w:r>
			            </w:p>
			        </xsl:if>
				</xsl:for-each-group>
	</xsl:template>
	<!-- CLIENTE.CONSOLIDADO END-->
	
	<!-- CLIENTE.DETAIL START-->
	<xsl:template match="CLIENTE" mode="detail">
		<xsl:for-each-group select="LINEA"
				group-by="concat(OPERACION, '|', VENCIMIENTOS/VENCIMIENTO[1]/MONEDA, '|', DESCRIPCION_LINEA)">
                <?audit suppress oracle.ide.xml.validation-error?>
				<xsl:sort select="OPERACION"/>
                <!--ENCABEZADO / CARATULA-->
					<xsl:for-each-group select="current-group()"
